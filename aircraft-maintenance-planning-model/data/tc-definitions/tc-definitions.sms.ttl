td:construct-maintenance-group-abbreviations
  sm:next td:rdfize-input-data ;
.
td:deploy-data_Return
  rdfs:label "deploy-tc-definitions-data" ;
.
td:import-scheme
  sm:next td:rdfize-input-data ;
.
td:rdfize-input-data
  kbss-module:has-output-graph-constraint [
      a sp:Ask ;
      sp:text """# 1.01 - column Task_Card has empty value
ASK
WHERE {
    ?t csvw:row/csvw:describes ?rd .
    FILTER NOT EXISTS {
          ?rd :Task_Card ?notExistValue .

    }
}""" ;
    ] ;
  kbss-module:has-output-graph-constraint [
      a sp:Ask ;
      sp:text """# 1.02 - column Aircraft model has empty value
# TODO discuss with MK
ASK
WHERE {
    ?t csvw:row/csvw:describes ?rd .
    FILTER NOT EXISTS {
          ?rd :Aircraft_model ?notExistValue .
    }
}""" ;
    ] ;
  kbss-module:has-output-graph-constraint [
      a sp:Ask ;
      sp:text """# 3 - found scope that is not defined in schema ontology
ASK
WHERE {
    ?t csvw:row/csvw:describes ?rd .
    ?rd :Scope ?maintenanceGroupAbbr .

    FILTER NOT EXISTS {
         ?maintenanceGroup  dl:has-simple-abbreviation ?maintenanceGroupAbbr .
    }
}""" ;
    ] ;
  km-tabular:data-prefix [
      a sp:str ;
      sp:arg1 : ;
    ] ;
  km-tabular:delimiter "	" ;
  sm:next :construct-csat-maintenance-data ;
.
td:transform-data_Return
  rdfs:label "transform-tc-definitions-data" ;
.
<http://onto.fel.cvut.cz/ontologies/csat/tc-definitions-0.1>
  a owl:Ontology ;
  owl:imports <http://onto.fel.cvut.cz/ontologies/csat-maintenance> ;
  owl:imports <http://onto.fel.cvut.cz/ontologies/csat/tabular-data-0.1> ;
  owl:imports <http://onto.fel.cvut.cz/ontologies/s-pipes-lib> ;
.
:construct-csat-maintenance-data
  a sml:ApplyConstruct ;
  sm:next :transform ;
  sml:constructQuery [
      a sp:Construct ;
      sp:text """# 1 - extract ontology
CONSTRUCT {
    <http://onto.fel.cvut.cz/ontologies/csat-maintenance/tc-definitions-12345> a owl:Ontology ;
        owl:imports <http://onto.fel.cvut.cz/ontologies/csat-maintenance> ;
    .
	
    ?taskCard  rdfs:subClassOf cm:task-type-definition ;
		cm:has-general-task-type ?taskType;
		cm:has-main-scope ?team;
		cm:task-for-phase ?phase;
		cm:has-mpdtask ?MPDTASK;
		rdfs:label ?title;
		dc_terms:description ?description;
		cm:notes ?notes;
		cm:id ?taskCardCode;
		cm:task-for-model ?aircraftModel;
		cm:task-for-area ?area;
		cm:required-el-power-restrictions ?elPower;
		cm:required-hyd-power-restrictions ?hydPower;
		cm:required-jack-restrictions ?jacks
	.
	
} WHERE {
		
	OPTIONAL {
		?rd :TASKNUMBER ?taskCardCode .
	}
	OPTIONAL {
		?rd :MPD_NUMBER ?MPDTASK .
	}
	OPTIONAL {
		?rd :TITLE ?title .
	}
	OPTIONAL {
		?rd :TEXT ?description .
	}
	OPTIONAL {
		?rd :NOTES ?notes .
	}
	OPTIONAL {
		?rd :TYPE_CODE ?taskType .
	} 
	OPTIONAL {
		?rd :HEADER ?general_task_type .
	}
	OPTIONAL {
		?rd :AREA_CODE ?area .
	}
	OPTIONAL {
		?rd :aircraft_model ?aircraftModel .
	}
	OPTIONAL {
		?rd :QUALIFICATION ?team .
	}
	OPTIONAL {
		?rd :PHASE ?phase .
	}
	OPTIONAL {
		?rd :El__Power ?elPower .
	}
	OPTIONAL {
		?rd :Hyd__Power ?hydPower .
	}
	OPTIONAL {
		?rd :Jacks ?jacks .
	}
	OPTIONAL {
		?rd :step ?step .
	}
	OPTIONAL {
		?rd :ignore ?ignore . # If value is bound to ignore, record should be ignored.
	}
	
	FILTER(?step = \"1\" && !BOUND(?ignore)) 
	
    BIND(dl:id-3(?aircraftModel, COALESCE(?taskCardCode, ?MPDTASK), ?step) as ?taskTypeDefinitionNormalizedId)
    
	BIND(dl:instance(cm:task-type-definition, ?taskTypeDefinitionNormalizedId) as ?taskTypeDefinitionIRI)
}""" ;
    ] ;
  sml:replace true ;
  rdfs:label "construct-csat-maintenance-data" ;
.
:deploy-tc-definitions-data
  a sm:Function ;
  sm:returnModule td:deploy-data_Return ;
  rdfs:subClassOf sm:Functions ;
.
:transform
  a sml:ApplyConstruct ;
  sm:next td:merge ;
  sml:replace false ;
.
:transform-tc-definitions-data
  a sm:Function ;
  sm:returnModule td:transform-data_Return ;
  rdfs:subClassOf sm:Functions ;
.

      a sp:Ask ;
      sp:text """# 1.01 - column AC_owner has empty value
ASK
WHERE {
    ?t csvw:row/csvw:describes ?rd .
    FILTER NOT EXISTS {
          ?rd :AC_owner ?notExistValue .

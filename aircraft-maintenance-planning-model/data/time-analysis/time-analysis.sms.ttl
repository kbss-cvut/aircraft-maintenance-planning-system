td:transform-data_Return
  rdfs:label "transform-time-analysis-data" ;
.
<http://onto.fel.cvut.cz/ontologies/csat/time-analysis-0.1>
  a owl:Ontology ;
  owl:imports <http://onto.fel.cvut.cz/ontologies/csat-maintenance> ;
  owl:imports <http://onto.fel.cvut.cz/ontologies/csat/tabular-data-0.1> ;
  owl:imports <http://onto.fel.cvut.cz/ontologies/s-pipes-lib> ;
.
:construct-csat-maintenance-data
  a sml:ApplyConstruct ;
  kbss-module:has-output-graph-constraint [
      a sp:Ask ;
      sp:text """# work-package has multiple scheduled-end-times
ASK  {
 ?workpackageIRI a cm:workpackage  ;
        cm:workpackage-scheduled-end-time ?t1 ;
        cm:workpackage-scheduled-end-time ?t2 ;
  .
  FILTER(?t1 != ?t2)
}""" ;
    ] ;
  sm:next :transform ;
  sml:constructQuery [
      a sp:Construct ;
      sp:text """# 1 - extract ontology
CONSTRUCT {
    <http://onto.fel.cvut.cz/ontologies/csat-maintenance/time-analysis-12345> a owl:Ontology ;
              owl:imports <http://onto.fel.cvut.cz/ontologies/csat-maintenance> .
        
    # workpackage description
    ?workpackageIRI a cm:workpackage, owl:NamedIndividual ;
              cm:id ?Workpackage ;
              cm:workpackage-scheduled-start-time ?WP_Start_date___scheduled ;
              cm:workpackage-scheduled-end-time ?WP_End_date___scheduled ;
              cm:workpackage-start-time ?WP_Start_date___real ;
              cm:workpackage-end-time ?WP_End_date___real ;
              cm:has-client ?client .

    ?taskExecution a cm:complex-execution, owl:NamedIndividual ;
              cm:is-part-of-workpackage ?workpackageIRI .

    ?workSession a cm:work-session ;
              cm:is-part-of-maintenance-task ?taskExecution ;
              cm:duration-in-hours ?durationInHours ;
              cm:has-scope ?maintenaceGroup .

    ?mechanic a cm:mechanic ;
               cm:performs ?workSession .

    ?maintenaceGroup a cm:maintenance-group ;
              cm:has-abbreviation ?Scope .
    
    ?client a cm:client, owl:NamedIndividual ;
              cm:id ?operator;
    .   
} WHERE {
     OPTIONAL {
        ?rd :AC ?AC .
     }
     ?rd :bookingno_i ?bookingno_i .
     OPTIONAL {
        ?rd :Employee_No ?Employee_No .
     }
     ?rd :user_sign ?user_sign .
     ?rd :Start_Date ?Start_Date .
     ?rd :Start_Time ?Start_Time .
     OPTIONAL {
        ?rd :End_Date ?End_Date .
     }
     ?rd :End_Time ?End_Time .
     ?rd :Duration_Full ?Duration_Full .
     ?rd :Scope ?Scope .
     OPTIONAL {
        ?rd :Shift_Group ?Shift_Group .
     }
     ?rd :Type ?Type .
     ?rd :WO_TC ?WO_TC .
     OPTIONAL {
        ?rd :TC_reference ?TC_reference .
     }
     OPTIONAL {
        ?rd :Closing_date ?Closing_date .
     }
     OPTIONAL {
        ?rd :Description ?Description .
     }
     OPTIONAL {
        ?rd :Workpackage ?Workpackage .
     }
     OPTIONAL {
        ?rd :A_C_model ?A_C_model .
     }
     OPTIONAL {
        ?rd :A_C_age ?A_C_age .
     }
     OPTIONAL {
        ?rd :WP_Start_date___scheduled ?WP_Start_date___scheduled .
     }
     OPTIONAL {
        ?rd :WP_End_date___scheduled ?WP_End_date___scheduled .
     }
     OPTIONAL {
        ?rd :WP_Start_date___real ?WP_Start_date___real .
     }
     OPTIONAL {
        ?rd :WP_End_date___real ?WP_End_date___real .
     }
     OPTIONAL {
        ?rd :Line ?Line .
     }
     ?rd :Real_date_of_booking_entry ?Real_date_of_booking_entry .
     OPTIONAL {
        ?rd :project ?project .
     }
     OPTIONAL {
        ?rd :AC_registration ?AC_registration .
     }
     OPTIONAL {
        ?rd :operator ?operator    .
     }

      ?maintenaceGroup rdfs:subClassOf+ cm:maintenance-group ;
          cm:has-abbreviation ?Scope .

     BIND(dl:instance(cm:workpackage, ?Workpackage) as ?workpackageIRI)
     BIND(dl:instance(cm:complex-execution, dl:id-2(?Workpackage, ?WO_TC)) as ?taskExecution)

     BIND(dl:instance(cm:mechanic, ?Employee_No) as ?mechanic)
     BIND(dl:instance(cm:work-session, ?bookingno_i) as ?workSession)
     BIND(xsd:decimal(?Duration_Full) as ?durationInHours)

     BIND(dl:instance(cm:client, ?operator) as ?client)
}""" ;
    ] ;
  sml:replace true ;
  rdfs:label "construct-csat-maintenance-data" ;
.
:deploy-time-analysis-data
  a sm:Function ;
  sm:returnModule td:deploy-data_Return ;
  rdfs:subClassOf sm:Functions ;
.
:import-scheme
  a sml:ImportRDFFromWorkspace ;
  sm:next :workaround-fix-missing-groups ;
  sml:baseURI "http://onto.fel.cvut.cz/ontologies/csat-maintenance" ;
  sml:ignoreImports true ;
  rdfs:label "import-scheme" ;
.
:transform
  a sml:ApplyConstruct ;
  sm:next td:merge ;
  sml:constructQuery [
      a sp:Construct ;
      sp:text """# 1 - convert endDate
CONSTRUCT {
     ?s  cm:end-time ?endDate .
} WHERE {
     ?s  cm:end-timeX ?endDateNotParsed .
#      BIND(spif:parseDate(?endDateNotParsed ,\"dd.MM.yyyy\") as ?endDate)
}""" ;
    ] ;
  sml:constructQuery [
      a sp:Construct ;
      sp:text """# 1 - convert issueDate
CONSTRUCT {
     ?s  cm:issue-time ?issueDate .
} WHERE {
     ?s  cm:issue-timeX ?issueDateNotParsed .
#      BIND(spif:parseDate(?issueDateNotParsed ,\"dd.MM.yyyy\") as ?issueDate)
}""" ;
    ] ;
  sml:replace false ;
.
:transform-time-analysis-data
  a sm:Function ;
  sm:returnModule td:transform-data_Return ;
  rdfs:subClassOf sm:Functions ;
.

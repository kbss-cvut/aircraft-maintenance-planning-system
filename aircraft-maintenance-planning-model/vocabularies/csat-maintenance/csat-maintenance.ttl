#################################################################
#    Annotation properties
#################################################################

###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/has_abbreviation
csat-maintenance:has_abbreviation rdf:type owl:AnnotationProperty ;
                                  rdfs:label "has abbreviation" .


#################################################################
#    Object Properties
#################################################################

###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/has_dmage
csat-maintenance:has_dmage rdf:type owl:ObjectProperty ;
                           rdfs:domain csat-maintenance:Planned_Task ;
                           rdfs:label "has damage" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/has_fiding
csat-maintenance:has_fiding rdf:type owl:ObjectProperty ;
                            rdfs:domain csat-maintenance:Finding ;
                            rdfs:label "has finding" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/has_follow_up_actions
csat-maintenance:has_follow_up_actions rdf:type owl:ObjectProperty ;
                                       rdfs:label "has follow up actions" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/has_scope
csat-maintenance:has_scope rdf:type owl:ObjectProperty ;
                           rdfs:domain csat-maintenance:Maintenance_task ;
                           rdfs:range csat-maintenance:Maintenance_Group ;
                           rdfs:label "has scope" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/leads
csat-maintenance:leads rdf:type owl:ObjectProperty ;
                       rdfs:comment "leads" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/member_of_group
csat-maintenance:member_of_group rdf:type owl:ObjectProperty ;
                                 rdfs:range csat-maintenance:Maintenance_Group ;
                                 rdfs:label "member of group" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/referes_to
csat-maintenance:referes_to rdf:type owl:ObjectProperty ;
                            rdfs:label "referes to" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/requires
csat-maintenance:requires rdf:type owl:ObjectProperty ;
                          rdfs:label "requires" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/task_type
csat-maintenance:task_type rdf:type owl:ObjectProperty ;
                           rdfs:domain csat-maintenance:Maintenance_task ;
                           rdfs:range csat-maintenance:Maintenance_task ;
                           rdfs:label "task type" .



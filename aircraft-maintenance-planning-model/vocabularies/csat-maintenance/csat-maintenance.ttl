                             rdfs:label "number" .


#################################################################
#    Classes
#################################################################

###  http://csat.cz/ontologies/maintenance-csat-onto/AV/INT
<http://csat.cz/ontologies/maintenance-csat-onto/AV/INT> rdf:type owl:Class ;
                                                         rdfs:subClassOf csat-maintenance:Maintenance_team ;
                                                         rdfs:label "AV/INT" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Access_step
csat-maintenance:Access_step rdf:type owl:Class ;
                             rdfs:subClassOf csat-maintenance:Task_steps ;
                             rdfs:label "Access step" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Adjusting
csat-maintenance:Adjusting rdf:type owl:Class ;
                           rdfs:subClassOf csat-maintenance:Task_Card ;
                           csat-maintenance:has_abbreviation "ADJ" ;
                           rdfs:label "Adjusting" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Aircraft
csat-maintenance:Aircraft rdf:type owl:Class ;
                          rdfs:subClassOf maintenance-schema:System ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty csat-maintenance:ATA_code ;
                                            owl:someValuesFrom xsd:integer
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty csat-maintenance:aircraft_age ;
                                            owl:someValuesFrom xsd:integer
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty csat-maintenance:aircraft_regestration_number ;
                                            owl:someValuesFrom xsd:integer
                                          ] .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Avionics_Mechanic_Group
csat-maintenance:Avionics_Mechanic_Group rdf:type owl:Class ;
                                         rdfs:subClassOf csat-maintenance:Heavy_Maintenance_Group ;
                                         csat-maintenance:has_abbreviation "AVIO" ;
                                         rdfs:label "Avionics Mechanic Group" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Borescope_Inspection
csat-maintenance:Borescope_Inspection rdf:type owl:Class ;
                                      rdfs:subClassOf csat-maintenance:Task_Card ;
                                      csat-maintenance:has_abbreviation "BSI" ;
                                      rdfs:label "Borescope Inspection" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Change
csat-maintenance:Change rdf:type owl:Class ;
                        rdfs:subClassOf csat-maintenance:Task_Card ;
                        csat-maintenance:has_abbreviation "CHG" ,
                                                          "Change" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Check
csat-maintenance:Check rdf:type owl:Class ;
                       rdfs:subClassOf csat-maintenance:Planned_Task ,
                                       csat-maintenance:Task_Card ;
                       csat-maintenance:has_abbreviation "CHK" ;
                       rdfs:label "Check" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Client
csat-maintenance:Client rdf:type owl:Class ;
                        rdfs:subClassOf ufo:organization ;
                        rdfs:label "Client" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Client_Extra__Order_task
csat-maintenance:Client_Extra__Order_task rdf:type owl:Class ;
                                          rdfs:subClassOf csat-maintenance:Maintenance_task ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty ufo:has-participant ;
                                                            owl:someValuesFrom csat-maintenance:Client
                                                          ] ;
                                          rdfs:label "Client Extra Order task" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Composite_Workshop__Support_Group
csat-maintenance:Composite_Workshop__Support_Group rdf:type owl:Class ;
                                                   rdfs:subClassOf csat-maintenance:Maintenance_Support_Group ;
                                                   csat-maintenance:has_abbreviation "Composite Workshop  Support Group" ;
                                                   rdfs:label "Composite Workshop  Support Group" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Damage
csat-maintenance:Damage rdf:type owl:Class ;
                        rdfs:subClassOf maintenance-schema:Failure_Event .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Detail_Visual_Inspection
csat-maintenance:Detail_Visual_Inspection rdf:type owl:Class ;
                                          rdfs:subClassOf csat-maintenance:Task_Card ;
                                          csat-maintenance:has_abbreviation "DVI" ;
                                          rdfs:label "Detail Visual Inspection" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Detailed_Inspection
csat-maintenance:Detailed_Inspection rdf:type owl:Class ;
                                     rdfs:subClassOf csat-maintenance:Task_Card ;
                                     csat-maintenance:has_abbreviation "DET" ;
                                     rdfs:label "Detailed Inspection" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Document
csat-maintenance:Document rdf:type owl:Class ;
                          rdfs:subClassOf maintenance-schema:Document .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/ENG
csat-maintenance:ENG rdf:type owl:Class ;
                     rdfs:subClassOf csat-maintenance:Maintenance_team ;
                     rdfs:label "ENG" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/El._Power
csat-maintenance:El._Power rdf:type owl:Class ;
                           rdfs:subClassOf csat-maintenance:Restriction ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty csat-maintenance:has_restriction_value ;
                                             owl:someValuesFrom rdfs:Literal
                                           ] ;
                           rdfs:comment """specifies the Electrical power system's state required for the Main step.
		- possible values (states): ON, OFF, / (no restriction)""" ;
                           rdfs:label "El. Power" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Engine_Mechanic_Group
csat-maintenance:Engine_Mechanic_Group rdf:type owl:Class ;
                                       rdfs:subClassOf csat-maintenance:Heavy_Maintenance_Group ;
                                       csat-maintenance:has_abbreviation "ING" ;
                                       rdfs:label "Engine Mechanic Group" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Exterior_Mechanic_Group
csat-maintenance:Exterior_Mechanic_Group rdf:type owl:Class ;
                                         rdfs:subClassOf csat-maintenance:Heavy_Maintenance_Group ;
                                         csat-maintenance:has_abbreviation "MECH-EXT" ;
                                         rdfs:label "Exterior Mechanic Group" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Final_step
csat-maintenance:Final_step rdf:type owl:Class ;
                            rdfs:subClassOf csat-maintenance:Task_steps ;
                            rdfs:label "Final step" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Finding
csat-maintenance:Finding rdf:type owl:Class ;
                         rdfs:subClassOf csat-maintenance:Observation ;
                         rdfs:label "Finding" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Finding__Damage
csat-maintenance:Finding__Damage rdf:type owl:Class ;
                                 rdfs:subClassOf csat-maintenance:Damage ,
                                                 csat-maintenance:Finding ,
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty ufo:is-part-of ;
                                                   owl:someValuesFrom csat-maintenance:Non_Planned_Task
                                                 ] ;
                                 rdfs:label "Finding  Damage" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Functional_Ttest
csat-maintenance:Functional_Ttest rdf:type owl:Class ;
                                  rdfs:subClassOf csat-maintenance:Task_Card ;
                                  csat-maintenance:has_abbreviation "FNC" ;
                                  rdfs:label "Functional Ttest" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/General
csat-maintenance:General rdf:type owl:Class ;
                         rdfs:subClassOf csat-maintenance:Task_Card ;
                         csat-maintenance:has_abbreviation "GEN" ;
                         rdfs:label "General" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/General_visual_inspection
csat-maintenance:General_visual_inspection rdf:type owl:Class ;
                                           rdfs:subClassOf csat-maintenance:Task_Card ;
                                           csat-maintenance:has_abbreviation "GVI" ;
                                           rdfs:label "General visual inspection" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Heavy_Maintenance_Group
csat-maintenance:Heavy_Maintenance_Group rdf:type owl:Class ;
                                         rdfs:subClassOf csat-maintenance:Maintenance_Group ;
                                         rdfs:label "Heavy Maintenance Group" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Hyd._Power
csat-maintenance:Hyd._Power rdf:type owl:Class ;
                            rdfs:subClassOf csat-maintenance:Restriction ;
                            rdfs:comment """- specifies the Hydraulic power system's state required for the Main step.
		- possible values (states): ON, OFF, / (no restriction)""" ;
                            rdfs:label "Hyd. Power" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/INT
csat-maintenance:INT rdf:type owl:Class ;
                     rdfs:subClassOf csat-maintenance:Maintenance_team ;
                     rdfs:label "INT" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Interior_Mechanic_Group
csat-maintenance:Interior_Mechanic_Group rdf:type owl:Class ;
                                         rdfs:subClassOf csat-maintenance:Heavy_Maintenance_Group ;
                                         csat-maintenance:has_abbreviation "MECH-INT" ;
                                         rdfs:label "Interior Mechanic Group" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Jack
csat-maintenance:Jack rdf:type owl:Class ;
                      rdfs:subClassOf csat-maintenance:Restriction ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty csat-maintenance:has_restriction_value ;
                                        owl:someValuesFrom rdfs:Literal
                                      ] ;
                      rdfs:comment """- specifies whether the Aircraft is required to be on or off the Jack for the Main step.
		- possible values (states): Yes, NO, / (no restriction)""" ;
                      rdfs:label "Jack" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Joiners_Support_Group
csat-maintenance:Joiners_Support_Group rdf:type owl:Class ;
                                       rdfs:subClassOf csat-maintenance:Maintenance_Support_Group ;
                                       csat-maintenance:has_abbreviation "JOI" ;
                                       rdfs:label "Joiners Support Group" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Landing_Gear_Mechanic_Group
csat-maintenance:Landing_Gear_Mechanic_Group rdf:type owl:Class ;
                                             rdfs:subClassOf csat-maintenance:Heavy_Maintenance_Group ;
                                             csat-maintenance:has_abbreviation "MECH-LDG" ;
                                             rdfs:label "Landing Gear Mechanic Group" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Line_Manager
csat-maintenance:Line_Manager rdf:type owl:Class ;
                              rdfs:subClassOf csat-maintenance:Maintenance_Manager ,
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty csat-maintenance:leads ;
                                                owl:someValuesFrom csat-maintenance:Team_Leader
                                              ] ;
                              rdfs:label "Line Manager" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Lubrication
csat-maintenance:Lubrication rdf:type owl:Class ;
                             rdfs:subClassOf csat-maintenance:Task_Card ;
                             csat-maintenance:has_abbreviation "LUB" ;
                             rdfs:label "Lubrication" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Main_step
csat-maintenance:Main_step rdf:type owl:Class ;
                           rdfs:subClassOf csat-maintenance:Task_steps ;
                           rdfs:label "Main step" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Maintenance_Group
csat-maintenance:Maintenance_Group rdf:type owl:Class ;
                                   rdfs:subClassOf ufo:physical-agent ;
                                   rdfs:label "Maintenance Group" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Maintenance_Manager
csat-maintenance:Maintenance_Manager rdf:type owl:Class ;
                                     rdfs:subClassOf ufo:physical-agent ;
                                     rdfs:label "Maintenance Manager" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Maintenance_Plan
csat-maintenance:Maintenance_Plan rdf:type owl:Class ;
                                  rdfs:subClassOf csat-maintenance:Document ;
                                  rdfs:comment "Description of the maintenance and inspection tasks to be performed at maintenance objects. The maintenance plans describe the dates and scope of the tasks." ;
                                  rdfs:label "Maintenance Plan" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Maintenance_Support_Group
csat-maintenance:Maintenance_Support_Group rdf:type owl:Class ;
                                           rdfs:subClassOf csat-maintenance:Maintenance_Group ;
                                           rdfs:label "Maintenance Support Group" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Maintenance_Work_Order
csat-maintenance:Maintenance_Work_Order rdf:type owl:Class ;
                                        rdfs:subClassOf csat-maintenance:Work_Order ;
                                        rdfs:comment "A maintenance work order provides details about maintenance, repair, or operations work, such as replacing a part, returning an asset to operating condition, or performing an inspection." ;
                                        rdfs:label "Maintenance Work Order" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/Maintenance_task
csat-maintenance:Maintenance_task rdf:type owl:Class ;
                                  rdfs:subClassOf ufo:complex-event ,
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty csat-maintenance:task_type ;
                                                    owl:someValuesFrom csat-maintenance:Maintenance_task
                                                  ] ,

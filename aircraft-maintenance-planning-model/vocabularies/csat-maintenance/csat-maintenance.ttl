        cm:maintenance-task .

cm:has-restriction-value a owl:DatatypeProperty ;
    rdfs:label "has restriction value" ;
    rdfs:domain cm:restriction .

cm:aircraft a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cm:aircraft-age ;
            owl:someValuesFrom xsd:integer ],
        [ a owl:Restriction ;
            owl:onProperty cm:aircraft-regestration-number ;
            owl:someValuesFrom xsd:integer ],
        [ a owl:Restriction ;
            owl:onProperty cm:ata-code ;
            owl:someValuesFrom xsd:integer ],
        ms:System .

cm:date a owl:DatatypeProperty ;
    rdfs:label "date" ;
    rdfs:range xsd:dateTimeStamp .

cm:planned-task a owl:Class ;
    rdfs:label "Planned Task" ;
    rdfs:subClassOf cm:client-extra--order-task .

cm:heavy-maintenance-group a owl:Class ;
    rdfs:label "Heavy Maintenance Group" ;
    rdfs:subClassOf cm:maintenance-group .

cm:maintenance-group a owl:Class ;
    rdfs:label "Maintenance Group" ;
    rdfs:subClassOf <http://onto.fel.cvut.cz/ontologies/ufo/physical-agent> .

cm:restriction a owl:Class ;
    rdfs:label "Restriction" ;
    rdfs:comment """Each task may have restrictions which must be met in order to perform the task
The restrictions are: 
		- El. Power - ON or OFF
		- Hyd. Power - ON or OFF
		- Jack - YES or NO""" ;
    rdfs:subClassOf cm:requirement .

cm:time a owl:DatatypeProperty ;
    rdfs:label "time" ;
    rdfs:range xsd:dateTime .

cm:maintenance-support-group a owl:Class ;
    rdfs:label "Maintenance Support Group" ;
    rdfs:subClassOf cm:maintenance-group .

cm:work-order a owl:Class ;
    rdfs:label "Work Order" ;
    rdfs:comment "A work order is a task order sent by a customer or organization, usually for a service technician." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty cm:work-order-text ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty cm:work-order-action-text ],
        cm:maintenance-task .

cm:maintenance-task a owl:Class ;
    rdfs:comment "Maintenance tasks indicate which action—or set of actions—a maintenance technician is supposed to perform to complete a work order" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cm:has-scope ;
            owl:someValuesFrom cm:maintenance-group ],
        [ a owl:Restriction ;
            owl:onProperty cm:task-type ;
            owl:someValuesFrom cm:maintenance-task ],
        [ a owl:Restriction ;
            owl:onProperty <http://onto.fel.cvut.cz/ontologies/ufo/is-performed-by> ;
            owl:someValuesFrom cm:maintenance-group ],
        [ a owl:Restriction ;
            owl:onProperty cm:task-type-identifier ;
            owl:someValuesFrom xsd:integer ],
        <http://onto.fel.cvut.cz/ontologies/ufo/complex-event> .

cm:task-card a owl:Class ;
    rdfs:label "Task Card" ;
    rdfs:subClassOf cm:planned-task .


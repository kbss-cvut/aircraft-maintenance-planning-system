###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/work-order
cm:work-order rdf:type owl:Class ;
              rdfs:subClassOf cm:maintenance-task ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty cm:work-order-text ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onDataRange xsd:string
                              ] ,

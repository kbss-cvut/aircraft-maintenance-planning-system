                                             rdfs:label "maintenance task start time" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/note
csat-maintenance:note rdf:type owl:DatatypeProperty ;
                      rdfs:label "note" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/number_of_planned_tasks
csat-maintenance:number_of_planned_tasks rdf:type owl:DatatypeProperty ;
                                         rdfs:subPropertyOf csat-maintenance:Ø§has_number ;
                                         rdfs:label "number of planned tasks" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/order
csat-maintenance:order rdf:type owl:DatatypeProperty ;
                       rdfs:label "order" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/task_type_identifier
csat-maintenance:task_type_identifier rdf:type owl:DatatypeProperty ;
                                      rdfs:domain csat-maintenance:Maintenance_task ;
                                      rdfs:range xsd:integer ;
                                      rdfs:label "task type identifier" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/time
csat-maintenance:time rdf:type owl:DatatypeProperty ;

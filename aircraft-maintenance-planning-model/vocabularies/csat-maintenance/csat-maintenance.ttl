csat-maintenance:workpackage_end_time rdf:type owl:DatatypeProperty ;
                                      rdfs:subPropertyOf csat-maintenance:time ;
                                      rdfs:label "workpackage end time" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/workpackage_scheduled_time
csat-maintenance:workpackage_scheduled_time rdf:type owl:DatatypeProperty ;
                                            rdfs:subPropertyOf csat-maintenance:time ;
                                            rdfs:label "workpackage scheduled time" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/workpackage_start__time
csat-maintenance:workpackage_start__time rdf:type owl:DatatypeProperty ;
                                         rdfs:subPropertyOf csat-maintenance:time ;
                                         rdfs:label "workpackage start time" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/workpackage_start_date
csat-maintenance:workpackage_start_date rdf:type owl:DatatypeProperty ;
                                        rdfs:subPropertyOf csat-maintenance:date ;
                                        rdfs:label "workpackage start date" .


###  http://onto.fel.cvut.cz/ontologies/csat-maintenance/worksession_end_date
csat-maintenance:worksession_end_date rdf:type owl:DatatypeProperty ;

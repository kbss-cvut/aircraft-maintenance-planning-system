:StatePartIdentifier a owl:Class ;
    rdfs:label "State Part Identifier"^^xsd:string ;
    ero:definition "A Geopolitical Entity Part Identifier that designates some State Part."^^xsd:string ;
    rdfs:subClassOf :GeopoliticalEntityPartIdentifier ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom <http://www.ontologylibrary.mil/CommonCore/Mid/GeospatialOntology#StatePart> ] .

:SubcontinentIdentifier a owl:Class ;
    rdfs:label "Subcontinent Identifier"^^xsd:string ;

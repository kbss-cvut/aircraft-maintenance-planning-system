:NautralLanguage a owl:Class ;
    rdfs:label "Natural Language"^^xsd:string ;
    ero:definition "A Language that is developed and evolves through use and repetition, rather than through conscious planning or premeditation."^^xsd:string ;
    ero:definition_source "https://en.wikipedia.org/wiki/Natural_language"^^xsd:string ;
    ero:example_of_usage "English"^^xsd:string,
        "Mandarin (Chinese)"^^xsd:string,
        "Spanish"^^xsd:string ;
    rdfs:subClassOf :Language .

:Nickname a owl:Class ;
    rdfs:label "Nickname"^^xsd:string ;

            owl:onProperty :designates ;
            owl:someValuesFrom time:Year ] .

:contains_token_information a owl:DatatypeProperty ;
    rdfs:label "contains token"^^xsd:string ;
    rdfs:domain :InformationBearingEntity ;
    rdfs:range xsd:string .

:designated_by a owl:InverseFunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "is designated by"^^xsd:string ;
    ero:definition "The inverse of designates, which relates an Entity to some Designative Information Content Entity."^^xsd:string ;
    rdfs:range :DesignativeInformationContentEntity ;
    rdfs:subPropertyOf :is_subject_of ;
    owl:inverseOf :designates .

:has_URI_value a owl:DatatypeProperty ;
    rdfs:label "has URI value"^^xsd:string ;
    rdfs:domain :InformationBearingEntity ;
    rdfs:range xsd:anyURI .

:has_boolean_value a owl:DatatypeProperty ;
    rdfs:label "has boolean value"^^xsd:string ;
    rdfs:domain :InformationBearingEntity ;
    rdfs:range xsd:boolean .

:has_date_value a owl:DatatypeProperty ;
    rdfs:label "has date value"^^xsd:string ;
    rdfs:domain :InformationBearingEntity ;
    rdfs:range xsd:date .

:has_datetime_value a owl:DatatypeProperty ;
    rdfs:label "has datetime value"^^xsd:string ;
    rdfs:domain :InformationBearingEntity ;
    rdfs:range xsd:dateTime .

:has_decimal_value a owl:DatatypeProperty ;
    rdfs:label "has decimal value"^^xsd:string ;
    rdfs:domain :InformationBearingEntity ;
    rdfs:range xsd:decimal .

:has_integer_value a owl:DatatypeProperty ;
    rdfs:label "has integer value"^^xsd:string ;
    rdfs:domain :InformationBearingEntity ;
    rdfs:range xsd:integer .

:has_text_value a owl:DatatypeProperty ;
    rdfs:label "has text value"^^xsd:string ;
    ero:definition "A relationship between an Information Bearing Entity and a string representation."^^xsd:string ;
    rdfs:domain :InformationBearingEntity ;
    rdfs:range xsd:string .

:is_excerpted_from a owl:ObjectProperty ;
    rdfs:label "is excerpted from"^^xsd:string ;
    ero:definition "An Information Bearing Entity b1 is excerpted from another Information Bearing Entity B2 iff b1 is part of some Information Bearing Entity B1 that is the bearer of some Information Content Entity C1, B2 is the bearer of some Information Content Entity C2, C1 is not identical with C2, b1 is the bearer of some Information Content Entity c1, b2 is an Information Bearing Entity that is part of B2 and b2 is the bearer of c1 (i.e. the same Information Content Entity as borne by b1)."^^xsd:string ;
    rdfs:domain :InformationBearingEntity ;
    rdfs:range :InformationBearingEntity .

:is_geospatial_coordinate_reference_system_of a owl:ObjectProperty ;
    rdfs:label "is geospatial coordinate reference system of"^^xsd:string ;
    rdfs:domain :GeospatialCoordinateReferenceSystem ;
    rdfs:range :InformationBearingEntity ;
    rdfs:subPropertyOf ero:inheres_in ;
    owl:inverseOf :uses_geospatial_coordinate_reference_system .

:is_mention_of a owl:ObjectProperty ;
    rdfs:label "is mention of"^^xsd:string ;
    ero:definition "An instance b1 of a Information Bearing Entity is mention of some instance c1 of some Entity or Event iff b1 is used as a reference to c1."^^xsd:string ;
    owl:inverseOf :is_mentioned_by .

:language_used_in a owl:ObjectProperty ;
    rdfs:label "languaged used in"^^xsd:string ;
    rdfs:domain :Language ;
    rdfs:range :InformationBearingEntity ;
    rdfs:subPropertyOf :prescribes ;
    owl:inverseOf :uses_language .

:represented_by a owl:InverseFunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "represented by"^^xsd:string ;
    ero:definition "The inverse of designates, which relates an Entity to some Representational Information Content Entity."^^xsd:string ;
    rdfs:range :RepresentationalInformationContentEntity ;
    rdfs:subPropertyOf :described_by ;
    owl:inverseOf :represents .

:uses_measurement_unit a owl:ObjectProperty ;
    rdfs:label "uses measurement unit"^^xsd:string ;
    ero:elucidation "An Information Bearing Entity carries the content of some corresponding Information Content Entity. In some cases of measurements, this content is composed of both some amount and the scale according to which the measurement was made."^^xsd:string ;
    rdfs:domain :InformationBearingEntity ;
    rdfs:range :MeasurementUnit ;
    rdfs:subPropertyOf ero:bearer_of ;
    owl:inverseOf ero:is_measurement_unit_of .

:CodeIdentifier a owl:Class ;
    rdfs:label "Code Identifier"^^xsd:string ;
    ero:alternative_label "Code ID"^^xsd:string,
        "ID Code"^^xsd:string,
        "Identifier Code"^^xsd:string ;
    ero:definition "A Non-Name Identifier that consists of a string of characters that was created and assigned according to an encoding system such that metadata can be derived from the identifier."^^xsd:string ;
    rdfs:subClassOf :NonNameIdentifier .

:DesignativeName a owl:Class ;
    rdfs:label "Designative Name"^^xsd:string ;
    ero:alternative_label "Name"^^xsd:string ;
    ero:definition "A Designative Information Content Entity that consists of a string of characters that designates an entity within a specified cultural or social namespace and which is typically a word or phrase in a natural language that has an accepted cultural or social significance."^^xsd:string ;
    rdfs:subClassOf :DesignativeInformationContentEntity ;
    owl:disjointWith :NonNameIdentifier .

:GeospatialBoundaryIdentifier a owl:Class ;
    rdfs:label "Geospatial Boundary Identifier"^^xsd:string ;
    ero:definition "A Spatial Region Identifier that designates some Geospatial Boundary."^^xsd:string ;

    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom <http://www.ontologylibrary.mil/CommonCore/Mid/GeospatialOntology#GeospatialBoundary> ] .

:MeasurementUnitOfGeocoordinate a owl:Class ;
    rdfs:label "Measurement Unit of Geocoordinate"^^xsd:string ;
    ero:definition "A Measurement Unit specifying the geospatial coordinates of a location."^^rdfs:Literal ;
    rdfs:subClassOf :MeasurementUnit .

:TemporalInstantIdentifier a owl:Class ;
    rdfs:label "Temporal Instant Identifier"^^xsd:string ;
    ero:definition "A Connected Temporal Region Identifier that designates some Temporal Instant."^^xsd:string ;
    rdfs:subClassOf :ConnectedTemporalRegionIdentifier ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom obo:BFO_0000148 ] .

:TemporalRegionIdentifier a owl:Class ;
    rdfs:label "Temporal Region Identifier"^^xsd:string ;
    ero:definition "A Designative Information Content Entity that designates some Temporal Region."^^xsd:string ;
    rdfs:subClassOf :DesignativeInformationContentEntity ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom obo:BFO_0000008 ] .

:is_a_interval_measurement_of a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "is a interval measurement of"^^xsd:string ;
    ero:definition "For all Types T1 and T2, if T1 is a interval measurement of T2 then there is some instance of T1, t1, that places some instance of T2, t2, onto a scale which has uniform intervals but has a zero value that does not correspond to an absence of the quality being measured."^^xsd:string ;
    ero:example_of_usage "a measurement of air temperature on the Celsius scale."^^xsd:string ;
    rdfs:domain :IntervalMeasurementInformationContentEntity ;
    rdfs:subPropertyOf :is_a_measurement_of ;
    owl:inverseOf :is_measured_by_interval .

:is_a_nominal_measurement_of a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "is a nominal measurement of"^^xsd:string ;
    ero:definition "For all Types T1 and T2, if T1 is a nominal measurement of T2 then there is some instance of T1, t1, that classifies some instance of T2, t2, relative to some set of shared, possibly arbitrary, characteristics."^^xsd:string ;
    ero:example_of_usage "classifying automobiles as sedans, coupes, hatchbacks, or convertibles"^^xsd:string,
        "classifying military intelligence as strategic, operational, or tactical"^^xsd:string,
        "classifying rocks as igneous, sedimentary, or metamorphic"^^xsd:string ;
    rdfs:domain :NominalMeasurementInformationContentEntity ;
    rdfs:subPropertyOf :is_a_measurement_of ;
    owl:inverseOf :is_measured_by_nominal .

:is_a_ordinal_measurement_of a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "is a ordinal measurement of"^^xsd:string ;
    ero:definition "For all Types T1 and T2, if T1 is a ordinal measurement of T2 then there is some instance of T1, t1, that places some instance of T2, t2, onto a rank order."^^xsd:string ;
    ero:example_of_usage "placing Geospatial Regions into a rank order of small, medium, large"^^xsd:string,
        "placing military units onto a readiness rank order of red, yellow, green"^^xsd:string ;
    rdfs:domain :OrdinalMeasurementInformationContentEntity ;
    rdfs:subPropertyOf :is_a_measurement_of ;
    owl:inverseOf :is_measured_by_ordinal .

:is_a_ratio_measurement_of a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "is a ratio measurement of"^^xsd:string ;
    ero:definition "For all Types T1 and T2, if T1 is a ratio measurement of T2 then there is some instance of T1, t1, that places some instance of T2, t2, onto a scale having equal unit values and a zero value that corresponds to the absence of the quality being measured."^^xsd:string ;
    ero:example_of_usage "the barometric pressure at 1,000 feet above sea level"^^xsd:string,
        "the measure of air temperature on the Kelvin scale"^^xsd:string,
        "the number of members in an Organization"^^xsd:string ;
    rdfs:domain :RatioMeasurementInformationContentEntity ;
    rdfs:subPropertyOf :is_a_measurement_of ;
    owl:inverseOf :is_measured_by_ratio .

:is_measured_by_interval a owl:InverseFunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "is measured by interval"^^xsd:string ;
    ero:definition "The inverse of is a interval measurement of, which relates an Entity to some Interval Measurement Information Content Entity."^^xsd:string ;
    rdfs:range :IntervalMeasurementInformationContentEntity ;
    rdfs:subPropertyOf :is_measured_by .

:is_measured_by_nominal a owl:InverseFunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "is measured by nominal"^^xsd:string ;
    ero:definition "The inverse of is a nominal measurement of, which relates an Entity to some Nominal Measurement Information Content Entity."^^xsd:string ;
    rdfs:range :NominalMeasurementInformationContentEntity ;
    rdfs:subPropertyOf :is_measured_by .

:is_measured_by_ordinal a owl:InverseFunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "is measured by ordinal"^^xsd:string ;
    ero:definition "The inverse of is a ordinal measurement of, which relates an Entity to some Ordinal Measurement Information Content Entity."^^xsd:string ;
    rdfs:range :OrdinalMeasurementInformationContentEntity ;
    rdfs:subPropertyOf :is_measured_by .

:is_measured_by_ratio a owl:InverseFunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "is measured by ratio"^^xsd:string ;
    ero:definition "The inverse of is a ratio measurement of, which relates an Entity to some Ratio Measurement Information Content Entity."^^xsd:string ;
    rdfs:range :RatioMeasurementInformationContentEntity ;
    rdfs:subPropertyOf :is_measured_by .

:is_mentioned_by a owl:ObjectProperty ;
    rdfs:label "is mentioned by"^^xsd:string ;
    ero:definition "An instance c1 of a Entity or Event is mentioned by an instance b1 of some Information Bearing Entity iff b1 is used as a reference to c1."^^xsd:string .

:prescribed_by a owl:ObjectProperty ;
    rdfs:label "prescribed by"^^xsd:string ;
    ero:definition "The inverse of presecribes, which relates an Entity to some Directive Information Content Entity."^^xsd:string ;
    rdfs:range :DirectiveInformationContentEntity ;
    rdfs:subPropertyOf :is_subject_of ;
    owl:inverseOf :prescribes .

:uses_geospatial_coordinate_reference_system a owl:ObjectProperty ;
    rdfs:label "uses geospatial coordinate reference system"^^xsd:string ;
    rdfs:domain :InformationBearingEntity ;
    rdfs:range :GeospatialCoordinateReferenceSystem ;
    rdfs:subPropertyOf ero:bearer_of .

:uses_language a owl:ObjectProperty ;
    rdfs:label "uses language"^^xsd:string ;
    rdfs:domain :InformationBearingEntity ;
    rdfs:range :Language ;
    rdfs:subPropertyOf :prescribed_by .

ero:is_measurement_unit_of a owl:ObjectProperty ;
    rdfs:label "is measurement unit of"^^xsd:string ;
    ero:elucidation "An Information Bearing Entity carries the content of some corresponding Information Content Entity. In some cases of measurements, this content is composed of both some amount and the scale according to which the measurement was made."^^xsd:string ;
    rdfs:domain :MeasurementUnit ;
    rdfs:range :InformationBearingEntity ;
    rdfs:subPropertyOf ero:inheres_in .

xsd:date a rdfs:Datatype .

:ConnectedTemporalRegionIdentifier a owl:Class ;
    rdfs:label "Connected Temporal Region Identifier"^^xsd:string ;
    ero:definition "A Temporal Region Identifier that designates some Connected Temporal Region."^^xsd:string ;
    ero:elucidation "A Connected Temporal Region is a Temporal Region that is contiguous such that there are no gaps in the spanned period of time."^^xsd:string ;
    rdfs:subClassOf :TemporalRegionIdentifier ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom obo:BFO_0000008 ] .

:GeopoliticalEntityIdentifier a owl:Class ;
    rdfs:label "Geopolitical Entity Identifier"^^xsd:string ;
    ero:definition "A Geospatial Region Identifier that desginates some Geopolitical Entity."^^xsd:string ;
    rdfs:subClassOf :GeospatialRegionIdentifier ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom <http://www.ontologylibrary.mil/CommonCore/Mid/GeospatialOntology#GeopoliticalEntity> ] .

:IntervalMeasurementInformationContentEntity a owl:Class ;
    rdfs:label "Interval Measurement Information Content Entity"^^xsd:string ;
    ero:definition "A Measurement Information Content Entity that consists of a symbol that places a Quality of an Entity onto some interval scale having no true zero value."^^xsd:string ;
    rdfs:subClassOf :MeasurementInformationContentEntity ;
    owl:disjointWith :NominalMeasurementInformationContentEntity,
        :OrdinalMeasurementInformationContentEntity,
        :RatioMeasurementInformationContentEntity ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :is_a_interval_measurement_of ;
            owl:someValuesFrom obo:BFO_0000001 ] .

:SiteIdentifier a owl:Class ;
    rdfs:label "Site Identifier"^^xsd:string ;
    ero:definition "A Designative Information Content Entity that designates some Site."^^xsd:string ;
    rdfs:subClassOf :DesignativeInformationContentEntity ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom obo:BFO_0000029 ] .

:described_by a owl:ObjectProperty ;
    rdfs:label "described by"^^xsd:string ;
    ero:definition "The inverse of describes, which relates an Entity to some Descriptive Information Content Entity."^^xsd:string ;
    rdfs:range :DescriptiveInformationContentEntity ;
    rdfs:subPropertyOf :is_subject_of ;
    owl:inverseOf :describes .

:represents a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "represents"^^xsd:string ;
    ero:definition "For all Types T1 and T2, if T1 represents T2 then there is some instance of T1, t1, that presents the characteristics by which some instance of T2, t2, can be recognized or visualized and moreover, there is a one-to-one correspondence between the components of t1 and t2."^^xsd:string ;
    ero:elucidation "The relationship that is being defined here is that between a photographic image and its object, or between a written transcript and the verbal event that it transcribes."^^xsd:string ;
    ero:example_of_usage "a court transcript represents a courtroom proceeding"^^xsd:string,
        "a photograph of the Statue of Liberty represents the Statue of Liberty"^^xsd:string,
        "a video of a sporting event represents that sporting event"^^xsd:string ;
    rdfs:domain :RepresentationalInformationContentEntity ;
    rdfs:subPropertyOf :describes .

:MeasurementUnit a owl:Class ;
    rdfs:label "Measurement Unit"^^xsd:string ;
    ero:definition "A Descriptive Information Content Entity that describes a definite magnitude of a physical quantity, defined and adopted by convention and/or by law, that is used as a standard for measurement of the same physical quantity."^^xsd:string ;
    ero:definition_source "Derived from http://en.wikipedia.org/wiki/Measurement_unit"^^xsd:string ;
    rdfs:subClassOf :DescriptiveInformationContentEntity .

:NominalMeasurementInformationContentEntity a owl:Class ;
    rdfs:label "Nominal Measurement Information Content Entity"^^xsd:string ;
    ero:definition "A Measurement Information Content Entity that consists of a symbol that classifies Entities according to some shared, possibly arbitrary, characteristic."^^xsd:string ;
    rdfs:subClassOf :MeasurementInformationContentEntity ;
    owl:disjointWith :OrdinalMeasurementInformationContentEntity,
        :RatioMeasurementInformationContentEntity ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :is_a_nominal_measurement_of ;
            owl:someValuesFrom obo:BFO_0000001 ] .

:NonNameIdentifier a owl:Class ;
    rdfs:label "Non-Name Identifier"^^xsd:string ;
    ero:alternative_label "ID"^^xsd:string,
        "Identifier"^^xsd:string ;
    ero:definition "A Designative Information Content Entity that consists of a string of characters that designates an entity within a specified namespace or context, is not a Designative Name, may be automatically or randomly generated, and typically has no preexisting cultural or social significance."^^xsd:string ;
    rdfs:subClassOf :DesignativeInformationContentEntity .

:RepresentationalInformationContentEntity a owl:Class ;
    rdfs:label "Representational Information Content Entity"^^xsd:string ;
    ero:definition "A Descriptive Information Content Entity that consists of a set of propositions or an image that represents some Entity."^^xsd:string ;
    rdfs:subClassOf :DescriptiveInformationContentEntity ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :represents ;
            owl:someValuesFrom obo:BFO_0000001 ] .

:SubNationalEntityIdentifier a owl:Class ;
    rdfs:label "Sub-National Entity Identifier"^^xsd:string ;
    ero:definition "A Geopolitical Entity Identifier that designates some Sub-National Entity."^^xsd:string ;
    rdfs:subClassOf :GeopoliticalEntityIdentifier ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom <http://www.ontologylibrary.mil/CommonCore/Mid/GeospatialOntology#FirstOrderAdministrativeRegion> ] .

:prescribes a owl:ObjectProperty ;
    rdfs:label "prescribes"^^xsd:string ;
    ero:definition "For all types T1 and T2, if T1 prescribes T2, then there is some instance of T1, t1, that serves as a rule or guide to some instance of T2, t2 (if T2 is a type of bfo:Occurrent) or that serves as a model for some instance of T2, t2 (if T2 is a type of bfo:Continuant)."^^xsd:string ;
    ero:example_of_usage "a blueprint serves as a model of some Artifact or Facility"^^xsd:string,
        "a professional code of conduct serves as a set of rules to be followed while acting in a role within that profession"^^xsd:string,
        "an Operations Plan serves as a guide for the tasks that need to be performed to achieve the Objectives of the Operation"^^xsd:string ;
    rdfs:domain :DirectiveInformationContentEntity ;
    rdfs:subPropertyOf :is_about .

:GeopoliticalEntityPartIdentifier a owl:Class ;
    rdfs:label "Geopolitical Entity Part Identifier"^^xsd:string ;
    ero:definition "A Geospatial Region Identifier that designates some Geopolitical Entity Part."^^xsd:string ;
    rdfs:subClassOf :GeospatialRegionIdentifier ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom <http://www.ontologylibrary.mil/CommonCore/Mid/GeospatialOntology#GeopoliticalEntityPart> ] .

:Language a owl:Class ;
    rdfs:label "Language"^^xsd:string ;
    ero:definition "A Directive Information Content Entity that prescribes a canonical format for communication."^^xsd:string ;
    rdfs:subClassOf :DirectiveInformationContentEntity .

:OrdinalMeasurementInformationContentEntity a owl:Class ;
    rdfs:label "Ordinal Measurement Information Content Entity"^^xsd:string ;
    ero:definition "A Measurement Information Content Entity that consists of a symbol that places an Entity into some rank order."^^xsd:string ;
    rdfs:subClassOf :MeasurementInformationContentEntity ;
    owl:disjointWith :RatioMeasurementInformationContentEntity ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :is_a_ordinal_measurement_of ;
            owl:someValuesFrom obo:BFO_0000001 ] .

:describes a owl:ObjectProperty ;
    rdfs:label "describes"^^xsd:string ;
    ero:definition "For all Types T1 and T2, if T1 describes T2 then there is some instance of T1, t1 that presents the characteristics by which some instance of T2, t2 can be recognized or visualized."^^xsd:string ;
    ero:example_of_usage "a newspaper article describes some current event"^^xsd:string,
        "a visitor's log describes some facility visit"^^xsd:string,
        "an accident report describes some accident"^^xsd:string ;
    rdfs:comment "It is possible that this relation should be a functional property, that is for all x, y, z if x describes y and x describes z then y = z. For example, if a financial report x describes the quarterly results of a company y and that same financial report describes the quarterly results of a company z, then it should be inferred that companies y and z are the same. We refrained from classifying the relation as a functional property on the concern that descriptions are multifaceted and so consequently it may be that the same report would contain descriptions of multiple entities."^^xsd:string ;
    rdfs:domain :DescriptiveInformationContentEntity ;
    rdfs:subPropertyOf :is_about .

:is_about a owl:ObjectProperty ;
    rdfs:label "is about"^^xsd:string ;
    ero:definition "A primitive (i.e. undefined) relationship between an Information Content Entity and some Entity."^^xsd:string ;
    ero:definition_source "Derived from IAO_0003136"^^xsd:string ;
    rdfs:domain :InformationContentEntity ;
    rdfs:range obo:BFO_0000001 ;
    owl:inverseOf :is_subject_of .

:is_subject_of a owl:ObjectProperty ;
    rdfs:label "is subject of"^^xsd:string ;
    ero:definition "The inverse of is about, which relates an Entity to some Information Content Entity."^^xsd:string ;
    rdfs:range :InformationContentEntity .

:GeospatialRegionIdentifier a owl:Class ;
    rdfs:label "Geospatial Region Identifier"^^xsd:string ;
    ero:definition "A Spatial Region Identifier that designates some Geospatial Region."^^xsd:string ;
    rdfs:subClassOf :SiteIdentifier ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom <http://www.ontologylibrary.mil/CommonCore/Mid/GeospatialOntology#GeospatialRegion> ] .

:is_a_measurement_of a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "is a measurement of"^^xsd:string ;
    ero:definition "For all Types T1 and T2, if T1 is a measurement of T2 then there is some instance of T1, t1, that estimates some characteristic of some instance of T2, t2, relative to some scale or classification scheme."^^xsd:string ;
    rdfs:comment "This object property, as well as all of its children are typified as functional properties. This means that for instances x, y, and z if x is a measurement of y and x is a measurement of z, then y = z."^^xsd:string ;
    rdfs:domain :MeasurementInformationContentEntity ;
    rdfs:subPropertyOf :describes ;
    owl:inverseOf :is_measured_by .

:is_measured_by a owl:InverseFunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "is measured by"^^xsd:string ;
    ero:definition "The inverse of is a measurement of, which relates an Entity to some Measurement Information Content Entity."^^xsd:string ;
    rdfs:range :MeasurementInformationContentEntity ;
    rdfs:subPropertyOf :described_by .

:DescriptiveInformationContentEntity a owl:Class ;
    rdfs:label "Descriptive Information Content Entity"^^xsd:string ;
    ero:alternative_label "Descriptive ICE"^^xsd:string ;
    ero:definition "An Information Content Entity that consists of a set of propositions that describe some Entity."^^xsd:string ;
    rdfs:subClassOf :InformationContentEntity ;
    owl:disjointWith :DesignativeInformationContentEntity,
        :DirectiveInformationContentEntity ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :describes ;
            owl:someValuesFrom obo:BFO_0000001 ] .

:DirectiveInformationContentEntity a owl:Class ;
    rdfs:label "Directive Information Content Entity"^^xsd:string ;
    ero:alternative_label "Directive ICE"^^xsd:string ;
    ero:definition "An Information Content Entity that consists of a set of propositions or images (as in the case of a blueprint) that prescribe some Entity."^^xsd:string ;
    rdfs:subClassOf :InformationContentEntity ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :prescribes ;
            owl:someValuesFrom obo:BFO_0000001 ] .

:GeospatialCoordinateReferenceSystem a owl:Class ;
    rdfs:label "Geospatial Coordinate Reference System"^^xsd:string ;
    ero:definition "A Descriptive Information Content Entity that describes a set of standards for determining the location of a point at or near the surface of the Earth."^^xsd:string ;
    rdfs:subClassOf :DescriptiveInformationContentEntity .

:MeasurementInformationContentEntity a owl:Class ;
    rdfs:label "Measurement Information Content Entity"^^xsd:string ;
    ero:alternative_label "Measurement ICE"^^xsd:string ;
    ero:definition "A Descriptive Information Content Entity that consists of a symbol that is a measure of the extent, dimensions, quanity, or quality of an Entity relative to some standard."^^xsd:string ;
    rdfs:subClassOf :DescriptiveInformationContentEntity ;
    owl:disjointWith :RepresentationalInformationContentEntity ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :is_a_measurement_of ;
            owl:someValuesFrom obo:BFO_0000001 ] .

:DesignativeInformationContentEntity a owl:Class ;
    rdfs:label "Designative Information Content Entity"^^xsd:string ;
    ero:alternative_label "Designative ICE"^^xsd:string ;
    ero:definition "An Information Content Entity that consists of a set of symbols that denote some Entity."^^xsd:string ;
    rdfs:subClassOf :InformationContentEntity ;
    owl:disjointWith :DirectiveInformationContentEntity ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom obo:BFO_0000001 ] .

:InformationContentEntity a owl:Class ;
    rdfs:label "Information Content Entity"^^xsd:string ;
    ero:alternative_label "ICE"^^xsd:string ;
    ero:definition "A Generically Dependent Continuant that generically depends on some Information Bearing Entity and stands in relation of aboutness to some Entity."^^xsd:string ;
    ero:definition_source "Derived from: http://purl.obolibrary.org/obo/IAO_0000030"^^xsd:string ;
    ero:elucidation "Information Content Entity is here intended to be a class of Entities whose instances are the informational content of Information Bearing Entities. For example, three instances of information bearers -- such as a bar chart, color-coded map, and a written report -- each of which lists the GDP of Countries for the year 2010 are each different bearers of the same information content. It is this content that is generically dependent upon its bearer. This treatment of Informational Content Entity (cf. the Information Artifact Ontology) leads to a principle of subtyping based upon the relationship that ICE's have with the Entity they are about rather than characteristics such as format, language, measurement scale, or media. The latter are treated here as being Qualities of bearers."^^xsd:string ;
    rdfs:subClassOf obo:BFO_0000031 ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :is_about ;
            owl:someValuesFrom obo:BFO_0000001 ] .

:RatioMeasurementInformationContentEntity a owl:Class ;
    rdfs:label "Ratio Measurement Information Content Entity"^^xsd:string ;
    ero:definition "A Measurement Information Content Entity that consists of a symbol that places a Quality of an Entity onto an interval scale having a true zero value."^^xsd:string ;
    rdfs:subClassOf :MeasurementInformationContentEntity ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :is_a_ratio_measurement_of ;
            owl:someValuesFrom obo:BFO_0000001 ] .

:TemporalIntervalIdentifier a owl:Class ;
    rdfs:label "Temporal Interval Identifier"^^xsd:string ;
    ero:definition "A Connected Temporal Region Identifier that designates some Temporal Interval."^^xsd:string ;
    rdfs:subClassOf :ConnectedTemporalRegionIdentifier ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom obo:BFO_0000038 ] .

:InformationBearingEntity a owl:Class ;
    rdfs:label "Information Bearing Entity"^^xsd:string ;
    ero:alternative_label "IBE"^^xsd:string ;
    ero:definition "An Object upon which an Information Content Entity generically depends."^^xsd:string ;
    ero:definition_source "Derived from: http://purl/obolibrary.org/obo/IAO_0000178"^^xsd:string ;
    rdfs:subClassOf obo:BFO_0000030 ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty ero:bearer_of ;
            owl:someValuesFrom :InformationContentEntity ] .

:designates a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "designates"^^xsd:string ;
    ero:definition "For all types T1 and T2, if T1 designates T2, then there is some instance of T1, t1, by which some instance of T2, t2,  is called, or classified, distinguished from others."^^xsd:string ;
    ero:example_of_usage "a URL identifies the location of a Web Page on the internet"^^xsd:string,
        "a person's name designates that person"^^xsd:string,
        "a vehicle identification number designates some vehicle"^^xsd:string ;
    rdfs:domain :DesignativeInformationContentEntity ;
    rdfs:subPropertyOf :is_about .


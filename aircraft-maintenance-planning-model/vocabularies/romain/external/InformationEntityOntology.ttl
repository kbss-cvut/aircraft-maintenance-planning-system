<http://www.ontologylibrary.mil/CommonCore/Mid/InformationEntityOntology> a owl:Ontology ;
    rdfs:label "Information Entity Ontology"@en ;
    ero:acronym "INFO"@en ;
    owl:imports <http://www.ontologylibrary.mil/CommonCore/Mid/GeospatialOntology>,
        <http://www.ontologylibrary.mil/CommonCore/Mid/TimeOntology>,
        <http://www.ontologylibrary.mil/CommonCore/Mid/UnitsOfMeasureOntology>,
        <http://www.ontologylibrary.mil/CommonCore/Upper/ExtendedRelationOntology> ;
    owl:versionInfo "March 28, 2017"^^xsd:date,
        "COPYRIGHT Â© 2015 CUBRC, Inc."^^xsd:string,
        "Version 0.90"^^xsd:string .

:Algorithm a owl:Class ;
    rdfs:label "Algorithm"^^xsd:string ;
    ero:definition "A Directive Information Content Entity that prescribes the inputs and output of mathematical functions as well as workflow of execution for achieving an predefined objective." ;
    ero:definition_source "http://purl.obolibrary.org/obo/IAO_0000064"^^xsd:string ;
    rdfs:subClassOf :DirectiveInformationContentEntity .

:ArbitraryIdentifier a owl:Class ;
    rdfs:label "Arbitrary Identifier"^^xsd:string ;
    ero:alternative_label "Arbitrary ID"^^xsd:string ;
    ero:definition "A Non-Name Identifier that consists of a string of characters that does not follow an encoding system."^^xsd:string ;
    rdfs:subClassOf :NonNameIdentifier ;
    owl:disjointWith :CodeIdentifier .

:ArtificialLanguage a owl:Class ;
    rdfs:label "Artificial Language"^^xsd:string ;
    ero:definition "A Language that is developed through conscious planning and premeditation, rather than one that was developed through use and repetition."^^xsd:string ;
    ero:definition_source "https://en.wikipedia.org/wiki/Constructed_language"^^xsd:string ;
    ero:example_of_usage "Esperanto"^^xsd:string,
        "Python Programming Language"^^xsd:string ;
    rdfs:subClassOf :Language .

:Certificate a owl:Class ;
    rdfs:label "Certificate" ;
    ero:definition "An Information Content Entity that attests to certain characteristics of an Object, Person, or Organization."^^xsd:string ;
    ero:definition_source "https://en.wikipedia.org/wiki/Certification"^^xsd:string ;
    rdfs:subClassOf :InformationContentEntity .

:CityIdentifier a owl:Class ;
    rdfs:label "City Identifier"^^xsd:string ;
    ero:definition "A Sub-National Entity Identifier that designates some City."^^xsd:string ;
    rdfs:subClassOf :SubNationalEntityIdentifier ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom <http://www.ontologylibrary.mil/CommonCore/Mid/GeospatialOntology#City> ] .

:CityPartIdentifier a owl:Class ;
    rdfs:label "City Part Identifier"^^xsd:string ;
    ero:definition "A Geopolitical Entity Part Identifier that designates some City Part."^^xsd:string ;
    rdfs:subClassOf :GeopoliticalEntityPartIdentifier ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom <http://www.ontologylibrary.mil/CommonCore/Mid/GeospatialOntology#CityPart> ] .

:ContinentIdentifier a owl:Class ;
    rdfs:label "Continent Identifier"^^xsd:string ;
    ero:definition "A Geospatial Region Identifier that designates some Continent."^^xsd:string ;
    rdfs:subClassOf :GeospatialRegionIdentifier ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom <http://www.ontologylibrary.mil/CommonCore/Mid/GeospatialOntology#Continent> ] .

:CountMeasurement a owl:Class ;
    rdfs:label "Count Measurement"^^xsd:string ;
    ero:definition "A Ratio Measurement Information Content Entity that measures the number of members of some aggregate."^^xsd:string ;
    rdfs:subClassOf :RatioMeasurementInformationContentEntity .

:CountryIdentifier a owl:Class ;
    rdfs:label "Country Identifier"^^xsd:string ;
    ero:definition "A Geopolitical Entity Identifier that designates some Country."^^xsd:string ;
    rdfs:subClassOf :GeopoliticalEntityIdentifier ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom <http://www.ontologylibrary.mil/CommonCore/Mid/GeospatialOntology#Country> ] .

:CountryPartIdentifier a owl:Class ;
    rdfs:label "Country Part Identifier"^^xsd:string ;
    ero:definition "A Geopolitical Entity Part Identifier that designates some Country Part."^^xsd:string ;
    rdfs:subClassOf :GeopoliticalEntityPartIdentifier ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom <http://www.ontologylibrary.mil/CommonCore/Mid/GeospatialOntology#CountryPart> ] .

:DateIdentifier a owl:Class ;
    rdfs:label "Date Identifier"^^xsd:string ;
    ero:definition "A Temporal Interval Identifier that designates some Day."^^xsd:string ;
    rdfs:subClassOf :TemporalIntervalIdentifier ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom time:Day ] .

:DecadeIdentifier a owl:Class ;
    rdfs:label "Decade Identifier"^^xsd:string ;
    ero:definition "A Temporal Interval Identifier that designates some Decade."^^xsd:string ;
    rdfs:subClassOf :TemporalIntervalIdentifier ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom time:Decade ] .

:DistanceMeasurement a owl:Class ;
    rdfs:label "Distance Measurement"^^xsd:string ;
    ero:definition "A Ratio Measurement Information Content Entity that describes a One-Dimensional Extent inhering in some Site that is externally connected to two Independent Continuants."^^xsd:string ;
    ero:elucidation "Displacement (vector) is the shortest possible distance (scalar) between two points. Further thought is needed to adequately handle measurements of distance traveled along a path, e.g., a circuitous path or an arc (such as the surface of a planet), versus the simple case of a direct, straight line between two points."^^xsd:string,
        "Distance is a measure between two reference points, which are located on or in, or in some manner part of, the two obejcts for which the length of the extent between is sought. For exmple, the center point of the indentation of a ball in the sand and the edge of the foul line, the forwardmost point on a car's bumper and the closest point on the 2-dimensional plane that serves as the fiat boundary of a crosswalk, the center of an antenna array and closest point on ground."^^xsd:string ;
    rdfs:subClassOf :RatioMeasurementInformationContentEntity .

:GeopoliticalEntityBorderIdentifier a owl:Class ;
    rdfs:label "Geopolitical Entity Border Identifier"^^xsd:string ;
    ero:definition "A Geospatial Boundary Identifier that designates some Geopolitical Entity Border."^^xsd:string ;
    rdfs:subClassOf :GeospatialBoundaryIdentifier ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom <http://www.ontologylibrary.mil/CommonCore/Mid/GeospatialOntology#GeopoliticalEntityBorder> ] .

:GeospatialLocationIdentifier a owl:Class ;
    rdfs:label "Geospatial Location Identifier"^^xsd:string ;
    ero:definition "A Geospatial Region Identifier that designates some Geospatial Location."^^xsd:string ;
    rdfs:subClassOf :GeospatialRegionIdentifier ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom <http://www.ontologylibrary.mil/CommonCore/Mid/GeospatialOntology#GeospatialLocation> ] .

:GeospatialRegionBoundingBoxIdentifierList a owl:Class ;
    rdfs:label "Geospatial Region Bounding Box Identifier List"^^xsd:string ;
    ero:definition "A Measurement Unit of Geocoordinate that is comprised of 4 pairs of coordinates, one for each of the 4 defining points (North, West, South, East) of a Geospatial Region Bounding Box."^^xsd:string ;
    rdfs:subClassOf :MeasurementUnitOfGeocoordinate .

:GlobalAreaReferenceSystem a :GeospatialCoordinateReferenceSystem,
        owl:NamedIndividual ;
    rdfs:label "Global Area Reference System"^^xsd:string ;
    ero:acronym "GARS"^^xsd:string .

:HourIdentifier a owl:Class ;
    rdfs:label "Hour Identifier"^^xsd:string ;
    ero:definition "A Temporal Interval Identifier that designates some Hour."^^xsd:string ;
    rdfs:subClassOf :TemporalIntervalIdentifier ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty :designates ;
            owl:someValuesFrom time:Hour ] .

:InformationQualityEntity a owl:Class ;
    rdfs:label "Information Quality Entity"^^xsd:string ;
    ero:alternative_label "IQE"^^xsd:string ;
    ero:definition "A Quality that concretizes some Information Content Entity."^^xsd:string ;
    rdfs:comment "Typically, an IQE will be a complex pattern made up of multiple qualities joined together spatially."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ero:inheres_in ;
            owl:someValuesFrom :InformationBearingEntity ],
        obo:BFO_0000019 .

:InformationStructureEntity a owl:Class ;
    rdfs:label "Information Structure Entity"^^xsd:string ;
    ero:definition "A Generically Dependent Continuant that is not an Information Content Entity (ICE) but which is complemented or complementable by some ICE to create another ICE."^^xsd:string ;
    ero:example_of_usage "a blank page in a document"^^xsd:string,
        "a cell in a spreadsheet"^^xsd:string ;
    rdfs:subClassOf obo:BFO_0000031 .

:MinuteIdentifier a owl:Class ;
    rdfs:label "Minute Identifier"^^xsd:string ;

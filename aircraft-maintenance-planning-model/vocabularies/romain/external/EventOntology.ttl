@prefix af_maint: <http://www.ontologylibrary.mil/CommonCore/Domain/AirForceAircraftMaintenanceOntology#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix ero: <http://www.ontologylibrary.mil/CommonCore/Upper/ExtendedRelationOntology#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.obofoundry.org/ro/ro.owl#has_part> a owl:ObjectProperty .

<http://www.obofoundry.org/ro/ro.owl#preceded_by> a owl:ObjectProperty .

<http://www.obofoundry.org/ro/ro.owl#precedes> a owl:ObjectProperty .

af_maint:ActOfArrival a owl:Class ;
    rdfs:label "Act of Arrival"^^xsd:string ;
    rdfs:subClassOf :ActOfLocationChange .

af_maint:ActOfDeparture a owl:Class ;
    rdfs:label "Act of Departure"^^xsd:string ;
    rdfs:subClassOf :ActOfLocationChange .

<http://www.ontologylibrary.mil/CommonCore/Mid/EventOntology> a owl:Ontology ;
    owl:imports <http://www.ontologylibrary.mil/CommonCore/Mid/InformationEntityOntology>,
        <http://www.ontologylibrary.mil/CommonCore/Upper/ExtendedRelationOntology> ;
    owl:versionInfo "COPYRIGHT Â© 2015 CUBRC, Inc."^^xsd:string,
        "October 24, 2016"^^xsd:string,
        "Version 0.90"^^xsd:string .

:ActOfAdvising a owl:Class ;
    rdfs:label "Act of Advising"^^xsd:string ;
    ero:definition "An Act of Directive Communication performed by providing advice or counsel to another agent."^^xsd:string ;
    ero:definition_source "Derived from: http://www.dictionary.com/browse/advising"^^xsd:string ;
    rdfs:subClassOf :ActOfDirectiveCommunication .

:ActOfApologizing a owl:Class ;
    rdfs:label "Act of Apologizing"^^xsd:string ;
    ero:definition "An Act of Expressive Communication performed by acknowledging and expressing regret for a fault, shortcoming, or failure."^^xsd:string ;
    ero:definition_source "Derived from: http://wordnetweb.princeton.edu/perl/webwn?s=apologizing"^^xsd:string ;
    rdfs:subClassOf :ActOfExpressiveCommunication .

:ActOfAppraisal a owl:Class ;
    rdfs:label "Act of Appraisal"^^xsd:string ;
    ero:definition "An Intentional Act that involves evaluating, estimating, or judging the nature, value, importance, condition, or quality of something or someone."^^xsd:string ;
    rdfs:subClassOf :IntentionalAct .

:ActOfArtifactModification a owl:Class ;
    rdfs:label "Act of Artifact Modification"^^xsd:string ;
    ero:definition "An Act of Artifact Processing in which an existing artifact is acted upon in a manner that changes one or more of its qualities, dispositions or functions or adds or removes one of these same."^^xsd:string ;
    rdfs:comment "Comment: Excluded from this class would be instaces of role change or creation such as introduction of an artifact as a piece of evidence in a trial or loading of artifacts onto a ship for transport."^^xsd:string ;
    rdfs:subClassOf :ActOfArtifactProcessing .

:ActOfAssassination a owl:Class ;
    rdfs:label "Act of Assassination"^^xsd:string ;
    ero:definition "An Act of Murder of a prominent person."^^xsd:string ;
    ero:definition_source "Derived from: JC3IEDM-MIRD-DMWG-Edition_3.0.2_20090514"^^xsd:string ;
    rdfs:subClassOf :ActOfMurder .

:ActOfAssignment a owl:Class ;
    rdfs:label "Act of Assignment"^^xsd:string ;
    ero:definition "An Act of Declarative Communication in which rights held by one party (the assignor) are transferred to another party (the assignee) with regard to a particular entity as specified by the details of the assignment, which is often prescribed by a contract."^^xsd:string ;
    ero:definition_source "Derived from: https://en.wikipedia.org/wiki/Assignment_%28law%29"^^xsd:string ;
    rdfs:subClassOf :ActOfDeclarativeCommunication .

:ActOfBuying a owl:Class ;
    rdfs:label "Act of Buying"^^xsd:string ;
    ero:definition "An Act of Purchasing wherein a Financial Instrument is used by an Agent (the buyer) to acquire ownership of a good from another agent (the seller)."^^xsd:string ;
    rdfs:subClassOf :ActOfPurchasing .

:ActOfCargoTransportation a owl:Class ;
    rdfs:label "Act of Cargo Transportation"^^rdfs:Literal ;
    ero:definition "An Act of Location Change involving the movement of manufactured goods through some Transportation Agent."^^xsd:string ;
    rdfs:subClassOf :ActOfLocationChange .

:ActOfChangeOfResidence a owl:Class ;
    rdfs:label "Act of Change of Residence"^^xsd:string ;
    ero:definition "An Act of Location Change involving one or more Persons moving from one place of residence to another."^^xsd:string ;
    rdfs:subClassOf :ActOfLocationChange .

:ActOfCommanding a owl:Class ;
    rdfs:label "Act of Commanding"^^xsd:string ;
    ero:definition "An Act of Directive Communication that exercises authoritative control or power over another agent's actions."^^xsd:string ;
    ero:definition_source "Derived from: http://wordnetweb.princeton.edu/perl/webwn?s=commanding (Edited 08-22-2016)"^^xsd:string ;
    rdfs:subClassOf :ActOfDirectiveCommunication .

:ActOfCondoling a owl:Class ;
    rdfs:label "Act of Condoling"^^xsd:string ;
    ero:definition "An Act of Expressive Communication performed by expressing sympathy or sorrow."^^xsd:string ;
    ero:definition_source "Derived from: http://www.thefreedictionary.com/condoling"^^xsd:string ;
    rdfs:subClassOf :ActOfExpressiveCommunication .

:ActOfConductingMassMediaInterview a owl:Class ;
    rdfs:label "Act of Conducting Mass Media Interview"^^xsd:string ;
    ero:definition "An Act of Mass Media Communication involving a conversation between a reporter and a person of public interest, used as a basis of a broadcast or publication."^^xsd:string ;
    ero:definition_source "Derived from: JC3IEDM-MIRD-DMWG-Edition_3.0.2_20090514"^^xsd:string ;
    rdfs:subClassOf :ActOfMassMediaCommunication .

:ActOfConfessing a owl:Class ;
    rdfs:label "Act of Confessing"^^xsd:string ;
    ero:definition "An Act of Representative Communication in which a person makes an admission of misdeeds or faults."^^xsd:string ;
    rdfs:subClassOf :ActOfRepresentativeCommunication .

:ActOfCongratulating a owl:Class ;
    rdfs:label "Act of Congratulating"^^xsd:string ;
    ero:definition "An Act of Expressive Communication performed by expressing vicarious pleasure to a person on the occasion of their success or good fortune."^^xsd:string ;
    ero:definition_source "Derived from: http://www.merriam-webster.com/dictionary/congratulate"^^xsd:string ;
    rdfs:subClassOf :ActOfExpressiveCommunication .

:ActOfContractFormation a owl:Class ;
    rdfs:label "Act of Contract Formation"^^rdfs:Literal ;
    ero:definition "An Act of Promising having a lawful object entered into voluntarily by two or more agents with legal capacity, each of whom intends to create one or more legal obligations between them."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Contract#Formation"^^xsd:string ;
    rdfs:subClassOf :ActOfPromising .

:ActOfDecoyUse a owl:Class ;
    rdfs:label "Act of Decoy Use"^^xsd:string ;
    ero:definition "An Act of Artifact Employment in which some Agent uses some Decoy."^^xsd:string ;
    rdfs:subClassOf :ActOfArtifactEmployment .

:ActOfDenying a owl:Class ;
    rdfs:label "Act of Denying"^^xsd:string ;
    ero:definition "An Act of Representative Communication performed by either asserting that something is not true or real or refusing to satisfy a request or desire."^^xsd:string ;
    ero:definition_source "Derived from: http://www.merriam-webster.com/dictionary/denial"^^xsd:string ;
    rdfs:subClassOf :ActOfRepresentativeCommunication .

:ActOfDonating a owl:Class ;
    rdfs:label "Act of Donating"^^xsd:string ;
    ero:definition "An Act of Financial Instrument Use wherein a Financial Instrument is tranfered by an agent (the donor) to another Agent (the Recipient) without return consideration."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Donation"^^xsd:string ;
    rdfs:subClassOf :ActOfFinancialInstrumentUse .

:ActOfEducationalTrainingAcquisition a owl:Class ;
    rdfs:label "Act of Educational Training Acquisition"^^xsd:string ;
    ero:definition "An Act of Training Acquisition performed by an Agent by acquiring knowledge of a curriculum from an Agent or communication medium that realizes a capacity derived from its affiliation with some Educational Organization."^^xsd:string ;
    rdfs:subClassOf :ActOfTrainingAcquisition .

:ActOfEducationalTrainingInstruction a owl:Class ;
    rdfs:label "Act of Educational Training Instruction"^^xsd:string ;
    ero:definition "An Act of Training Instruction performed by an Agent who realizes a capacity derived from the Agent's affiliation with some Educational Organization by imparting knowledge of a curriculum to an Agent either directly or indirectly through some communication medium."^^xsd:string ;
    rdfs:subClassOf :ActOfTrainingInstruction .

:ActOfEncounter a owl:Class ;
    rdfs:label "Act of Encounter"^^xsd:string ;
    ero:definition "An Act of Association wherein two or more Persons meet in a casual or unplanned manner."^^xsd:string ;
    ero:definition_source "Derived from: http://wordnetweb.princeton.edu/perl/webwn?s=encounter (Sense Key: Encounter%2:38:00)"^^xsd:string ;
    rdfs:subClassOf :ActOfAssociation .

:ActOfEntertainment a owl:Class ;
    rdfs:label "Act of Entertainment"^^xsd:string ;
    ero:definition "An Intentional Act consisting of any activity which provides a diversion or permits people to amuse themselves."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Entertainment"^^xsd:string ;
    rdfs:subClassOf :IntentionalAct .

:ActOfEspionage a owl:Class ;
    rdfs:label "Act of Espionage"^^xsd:string ;
    ero:definition "An Act of Intelligence Gathering involving the obtaining of information that is considered secret or confidential without the permission of the holder of the information."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Espionage"^^xsd:string ;
    rdfs:subClassOf :ActOfIntelligenceGathering .

:ActOfFacilityUse a owl:Class ;
    rdfs:label "Act of Facility Use"^^xsd:string ;
    ero:definition "An Act of Artifact Employment in which an Agent makes use of some Facility."^^xsd:string ;
    rdfs:subClassOf :ActOfArtifactEmployment .

:ActOfFinancialDeposit a owl:Class ;
    rdfs:label "Act of Financial Deposit"^^xsd:string ;
    ero:definition "An Act of Financial Instrument Use wherein a Financial Instrument is transferred by an agent (the Depositor) to another Agent to create a liability to be repaid to the Depositor through an Act of Financial Withdrawal."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Deposit_(bank)"^^xsd:string ;
    rdfs:subClassOf :ActOfFinancialInstrumentUse .

:ActOfFinancialWithdrawal a owl:Class ;
    rdfs:label "Act of Financial Withdrawal"^^xsd:string ;
    ero:definition "An Act of Financial Instrument Use wherein a Financial Instrument is transferred by an agent to another Agent (the Depositor) from an account created by an earlier Act of Financial Deposit performed by the Depositor."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Deposit_(bank)"^^xsd:string ;
    rdfs:subClassOf :ActOfFinancialInstrumentUse .

:ActOfFunding a owl:Class ;
    rdfs:label "Act of Funding"^^xsd:string ;
    ero:definition "An Act of Financial Instrument Use in which an agent provides a sum of money to another agent for a particular purpose."^^xsd:string ;
    rdfs:subClassOf :ActOfFinancialInstrumentUse .

:ActOfGovernment a owl:Class ;
    rdfs:label "Act of Government"^^rdfs:Literal ;
    ero:definition "An Act which is generated or supported by some Geopolitical Entity and enforced by some Government Agent."^^xsd:string ;
    rdfs:subClassOf :IntentionalAct .

:ActOfIdentifying a owl:Class ;
    rdfs:label "Act of Identifying"^^xsd:string ;
    ero:definition "An Act of Representative Communication performed by asserting who or what a particular person or thing is."^^xsd:string ;
    ero:definition_source "Derived from: http://www.dictionary.com/browse/identify"^^xsd:string ;
    rdfs:subClassOf :ActOfRepresentativeCommunication .

:ActOfInterpersonalRelationship a owl:Class ;
    rdfs:label "Act of Interpersonal Relationship"^^xsd:string ;
    ero:definition "An Act of Association between two or more Persons that may range from fleeting to enduring."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Interpersonal_relationship"^^xsd:string ;
    rdfs:subClassOf :ActOfAssociation .

:ActOfInviting a owl:Class ;
    rdfs:label "Act of Inviting"^^xsd:string ;
    ero:definition "An Act of Commissive Communication performed by requesting the presence or participation of the audience, and which may include offering an incentive or inducement."^^xsd:string ;
    ero:definition_source "Derived from: http://www.merriam-webster.com/dictionary/invite"^^xsd:string ;
    rdfs:subClassOf :ActOfCommissiveCommunication .

:ActOfIssuingMassMediaArticle a owl:Class ;
    rdfs:label "Act of Issuing Mass Media Article"^^xsd:string ;
    ero:definition "An Act of Mass Media Communication involving sending forth, distributing, or putting into circulation a non-fictional essay, especially one included with others in a newspaper, magazine, or journal."^^xsd:string ;
    ero:definition_source "Derived from: JC3IEDM-MIRD-DMWG-Edition_3.0.2_20090514"^^xsd:string ;
    rdfs:subClassOf :ActOfMassMediaCommunication .

:ActOfIssuingMassMediaDocumentary a owl:Class ;
    rdfs:label "Act of Issuing Mass Media Documentary"^^xsd:string ;
    ero:definition "An Act of Mass Media Communication involving sending forth, distributing, or putting into circulation information that provides a factual record or report."^^xsd:string ;
    ero:definition_source "Derived from: JC3IEDM-MIRD-DMWG-Edition_3.0.2_20090514"^^xsd:string ;
    rdfs:subClassOf :ActOfMassMediaCommunication .

:ActOfIssuingMassMediaPressRelease a owl:Class ;
    rdfs:label "Act of Issuing Mass Media Press Release"^^xsd:string ;
    ero:definition "An Act of Mass Media Communication involving sending forth, distributing, or putting into circulation an official statement issued to one or more members of the media for the purpose of announcing newsworthy information."^^xsd:string ;
    ero:definition_source "Derived from: JC3IEDM-MIRD-DMWG-Edition_3.0.2_20090514"^^xsd:string ;
    rdfs:subClassOf :ActOfMassMediaCommunication .

:ActOfLegalInstrumentUse a owl:Class ;
    rdfs:label "Act of Legal Instrument Use"^^xsd:string ;
    ero:definition "An Act of Artifact Employment in which some Agent uses some Legal Instrument."^^xsd:string ;
    rdfs:subClassOf :ActOfArtifactEmployment .

:ActOfLoaning a owl:Class ;
    rdfs:label "Act of Loaning"^^xsd:string ;
    ero:definition "An Act of Financial Instrument Use wherein a Financial Instrument is given by an agent (the Creditor) to another Agent (the Debtor) in order to receive Repayments from the Debtor which are of greater Financial Value."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Loan"^^xsd:string ;
    rdfs:subClassOf :ActOfFinancialInstrumentUse .

:ActOfMeeting a owl:Class ;
    rdfs:label "Act of Meeting"^^xsd:string ;
    ero:definition "An Act of Association wherein two or more Persons assemble for some common purpose."^^xsd:string ;
    ero:definition_source "Derived from: http://wordnetweb.princeton.edu/perl/webwn?s=meet (Sense Key: meet%2:41:00, get together%2:41:00)"^^xsd:string ;
    rdfs:subClassOf :ActOfAssociation .

:ActOfMilitaryForce a owl:Class ;
    rdfs:label "Act of Military Force"^^xsd:string ;
    ero:definition "An Intentional Act of employing a Military Force to achieve some desired result."^^xsd:string ;
    rdfs:subClassOf :IntentionalAct .

:ActOfMilitaryService a owl:Class ;
    rdfs:label "Act of Military Service"^^xsd:string ;
    ero:definition "An Act of Employment wherein a Person serves as a member of a Military Force, whether voluntarily or by conscription."^^xsd:string ;
    ero:definition_source "Derived from: http://www.collinsdictionary.com/dictionary/english/military-service"^^xsd:string ;
    rdfs:subClassOf :ActOfEmployment .

:ActOfMilitaryTrainingAcquisition a owl:Class ;
    rdfs:label "Act of Military Training Acquisition"^^xsd:string ;
    ero:definition "An Act of Training Acquisition performed by an Agent by acquiring knowledge of the tactics, techniques, and/or strategies of Military Operations from an Agent or communication medium that realizes a capacity derived from its affiliation with a Military Organization."^^xsd:string ;
    rdfs:subClassOf :ActOfTrainingAcquisition .

:ActOfMilitaryTrainingInstruction a owl:Class ;
    rdfs:label "Act of Military Training Instruction"^^xsd:string ;
    ero:definition "An Act of Training Instruction performed by an Agent who realizes a capacity derived from the Agent's affiliation with some Military Organization by imparting knowledge of the tactics, techniques, and/or strategies of Military Operations to an Agent either directly or indirectly through some communication medium."^^xsd:string ;
    rdfs:subClassOf :ActOfTrainingInstruction .

:ActOfOathTaking a owl:Class ;
    rdfs:label "Act of Oath Taking"^^xsd:string ;
    ero:definition "An Act of Commissive Communication performed by stating or promising, usually calling upon something or someone that the oath maker considers sacred."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Oath"^^xsd:string ;
    rdfs:subClassOf :ActOfCommissiveCommunication .

:ActOfOfficialDocumentation a owl:Class ;
    rdfs:label "Act of Official Documentation"^^xsd:string ;
    ero:definition "An Act of Declarative Communication in which an Agent records some information for official use by another Agent."^^xsd:string ;
    rdfs:subClassOf :ActOfDeclarativeCommunication .

:ActOfOwnership a owl:Class ;
    rdfs:label "Act of Ownership"^^xsd:string ;
    ero:definition "An Act of Possession that includes an agent having certain rights and duties over the property owned."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Ownership"^^xsd:string ;
    rdfs:comment "The relation between the owner and property may be private, collective, or common and the property may be objects, land, real estate, or intellectual property."^^xsd:string ;
    rdfs:subClassOf :ActOfPossession .

:ActOfPersonalCommunication a owl:Class ;
    rdfs:label "Act of Personal Communication"^^xsd:string ;
    ero:definition "An Act of Communication having a small audience."^^xsd:string ;
    rdfs:subClassOf :ActOfCommunication .

:ActOfPilgrimage a owl:Class ;
    rdfs:label "Act of Pilgrimage"^^xsd:string ;
    ero:definition "An Act of Travel to a location of importance to a person's beliefs and faith."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Pilgrimage"^^xsd:string ;
    rdfs:subClassOf :ActOfTravel .

:ActOfPlanning a owl:Class ;
    rdfs:label "Act of Planning"^^xsd:string ;
    ero:definition "An Intentional Act that involves making a Plan to achieve some specified Objective."^^xsd:string ;
    rdfs:subClassOf :IntentionalAct .

:ActOfPortionOfMaterialConsumption a owl:Class ;
    rdfs:label "Act of Portion of Material Consumption"^^xsd:string ;
    ero:definition "An Act of Artifact Employment in which a Portion of Material is expended."^^xsd:string ;
    rdfs:subClassOf :ActOfArtifactEmployment .

:ActOfPositionChange a owl:Class ;
    rdfs:label "Act of Position Change"^^xsd:string ;
    ero:definition "An Act of Motion in which the position (i.e. the orientation, alignment, or arrangement) of some Object or of one or more of an Object's parts is changed by some Agent."^^xsd:string ;
    ero:example_of_usage "a top spinning in place"^^xsd:string,
        "adjusting your posture"^^xsd:string,
        "raising your left arm"^^xsd:string,
        "swivelling your office chair to face the window"^^xsd:string ;
    rdfs:comment "An Act of Position Change does not entail a change of location."^^xsd:string ;
    rdfs:subClassOf :ActOfMotion .

:ActOfPropaganda a owl:Class ;
    rdfs:label "Act of Propaganda"^^xsd:string ;
    ero:definition "An Act of Deceptive Communication Propaganda intended to influence the attitude of a mass audience toward some cause or position by presenting facts selectively (thus possibly lying by omission) or by using loaded messages to produce an emotional rather than rational response to the information presented."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Propaganda"^^xsd:string ;
    rdfs:subClassOf :ActOfDeceptiveCommunication .

:ActOfPublishingMassMediaArticle a owl:Class ;
    rdfs:label "Act of Publishing Mass Media Article"^^xsd:string ;
    ero:definition "An Act of Mass Media Communication involving the preparation and public issuance of a non-fictional essay, especially one included with others in a newspaper, magazine, journal, etc."^^xsd:string ;
    ero:definition_source "Derived from: JC3IEDM-MIRD-DMWG-Edition_3.0.2_20090514"^^xsd:string ;
    rdfs:subClassOf :ActOfMassMediaCommunication .

:ActOfPublishingMassMediaDocumentary a owl:Class ;
    rdfs:label "Act of Publishing Mass Media Documentary"^^xsd:string ;
    ero:definition "An Act of Mass Media Communication involving the preparation and public issuance of information that provides a factual record or report."^^xsd:string ;
    ero:definition_source "Derived from: JC3IEDM-MIRD-DMWG-Edition_3.0.2_20090514"^^xsd:string ;
    rdfs:subClassOf :ActOfMassMediaCommunication .

:ActOfPublishingMassMediaPressRelease a owl:Class ;
    rdfs:label "Act of Publishing Mass Media Press Release"^^xsd:string ;
    ero:definition "An Act of Mass Media Communication involving the preparation and public issuance of an official statement issued to one or more members of the media for the purpose of announcing newsworthy information."^^xsd:string ;
    ero:definition_source "Derived from: JC3IEDM-MIRD-DMWG-Edition_3.0.2_20090514"^^xsd:string ;
    rdfs:subClassOf :ActOfMassMediaCommunication .

:ActOfReconnaissance a owl:Class ;
    rdfs:label "Act of Reconnaissance"^^xsd:string ;
    ero:definition "An Act of Intelligence Gathering used to determine an Agent's disposition and intention; the composition and capabilities of Equipment and Facilities, and the surrounding landforms and weather conditions."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Reconnaissance"^^xsd:string ;
    rdfs:subClassOf :ActOfIntelligenceGathering .

:ActOfReligiousGroupAffiliation a owl:Class ;
    rdfs:label "Act of Religious Group Affiliation"^^xsd:string ;
    ero:definition "An Act of Association wherein some Person belongs to some Religious Demonination or sub-Denomination."^^xsd:string ;
    rdfs:subClassOf :ActOfAssociation .

:ActOfReligiousTrainingAcquisition a owl:Class ;
    rdfs:label "Act of Religious Training Acquisition"^^xsd:string ;
    ero:definition "An Act of Training Acquisition performed by an Agent by acquiring knowledge of the tenets and/or practices of a religion from an Agent or communication medium that realizes a capacity derived from its affiliation with a Religious Organization."^^xsd:string ;
    rdfs:subClassOf :ActOfTrainingAcquisition .

:ActOfReligiousTrainingInstruction a owl:Class ;
    rdfs:label "Act of Religious Training Instruction"^^xsd:string ;
    ero:definition "An Act of Training Instruction performed by an Agent who realizes a capacity derived from the Agent's affiliation with some Religious Organization by imparting knowledge of the tenets and/or practices of a religion to an Agent either directly or indirectly through some communication medium."^^xsd:string ;
    rdfs:subClassOf :ActOfTrainingInstruction .

:ActOfReligiousWorship a owl:Class ;
    rdfs:label "Act of Religious Worship"^^xsd:string ;
    ero:definition "An Act of Ceremony wherein there occurs acts of religious devotion usually directed towards a deity."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Worship (Sense Key: Worship%2:42:00 or Worship%1:04:00::)"^^xsd:string ;
    rdfs:subClassOf :ActOfCeremony .

:ActOfRemuneration a owl:Class ;
    rdfs:label "Act of Remuneration"^^xsd:string ;
    ero:definition "An Act of Purchasing wherein a Financial Instrument is used by an Agent (the Employer) to compensate another Agent (the Employee) for the services they perform for the Employer."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Remuneration"^^xsd:string ;
    rdfs:comment "Comment: Remuneration normally consists of salary or hourly wages, but also applies to commission, stock options, fringe benefits, etc."^^xsd:string ;
    rdfs:subClassOf :ActOfPurchasing .

:ActOfRenting a owl:Class ;
    rdfs:label "Act of Renting"^^rdfs:Literal ;
    ero:definition "An Act of Financial Instrument Use wherein a Financial Instrument is used by an Agent (the renter) as a payment to acquire temporary possession or use of a good or property that is owned or controlled by another agent (the seller)."^^xsd:string ;
    rdfs:subClassOf :ActOfPurchasing .

:ActOfRepayment a owl:Class ;
    rdfs:label "Act of Repayment"^^xsd:string ;
    ero:definition "An Act of Financial Instrument Use wherein a Financial Instrument is returned by an agent (the Debtor) to another Agent (the Creditor) to decrease the amount owed to the Creditor from an earlier Act of Loaning."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Loan"^^xsd:string ;
    rdfs:subClassOf :ActOfFinancialInstrumentUse .

:ActOfReporting a owl:Class ;
    rdfs:label "Act of Reporting"^^xsd:string ;
    ero:definition "An Act of Representative Communication performed by giving a detailed account or statement."^^xsd:string ;
    ero:definition_source "Derived from: http://www.merriam-webster.com/dictionary/report"^^xsd:string ;
    rdfs:subClassOf :ActOfRepresentativeCommunication .

:ActOfRequesting a owl:Class ;
    rdfs:label "Act of Requesting"^^xsd:string ;
    ero:definition "An Act of Directive Communication performed by asking for something."^^xsd:string ;
    ero:definition_source "Derived from: http://www.merriam-webster.com/dictionary/request"^^xsd:string ;
    rdfs:subClassOf :ActOfDirectiveCommunication .

:ActOfResiding a owl:Class ;
    rdfs:label "Act of Residing"^^xsd:string ;
    ero:definition "An Act of Inhabitancy in which a Person lives in a dwelling permanently or for an extended period of time."^^xsd:string ;
    ero:definition_source "http://www.thefreedictionary.com/reside"^^xsd:string ;
    rdfs:subClassOf :ActOfInhabitancy .

:ActOfSocialGroupMembership a owl:Class ;
    rdfs:label "Act of Social Group Membership"^^xsd:string ;
    ero:definition "An Act of Association wherein a Person belongs to some Social Group."^^xsd:string ;
    rdfs:subClassOf :ActOfAssociation .

:ActOfSocialMovement a owl:Class ;
    rdfs:label "Act of Social Movement"^^xsd:string ;
    ero:definition "A Social Act composed of a series of performances, displays and campaigns directed at implementing, resisting or undoing a change in society."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Social_movement"^^xsd:string ;
    rdfs:subClassOf :SocialAct .

:ActOfSojourn a owl:Class ;
    rdfs:label "Act of Sojourn"^^xsd:string ;
    ero:definition "An Act of Inhabitancy in which a Person lives in a dwelling temporarily, often during stages of an Act of Travel."^^xsd:string ;
    rdfs:subClassOf :ActOfInhabitancy .

:ActOfSuicide a owl:Class ;
    rdfs:label "Act of Suicide"^^xsd:string ;
    ero:alternative_label "Suicide"^^xsd:string ;
    ero:definition "An Act of Violence in which some Agent intentionally and voluntarily causes their own death."^^xsd:string ;
    ero:definition_source "Derived from: http://www.merriam-webster.com/dictionary/suicide"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ero:is_cause_of ;
            owl:someValuesFrom :Death ],
        :ActOfViolence .

:ActOfTerrorism a owl:Class ;
    rdfs:label "Act of Terrorism"^^xsd:string ;
    ero:definition "An Act of Violence which creates fear and which is prescribed by religious, political or ideological objectives, and which deliberately target or disregard the safety of civilians and are committed by members of non-government organizations."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Terrorism"^^xsd:string ;
    rdfs:subClassOf :IntentionalAct .

:ActOfTerroristTrainingAcquisition a owl:Class ;
    rdfs:label "Act of Terrorist Training Acquisition"^^xsd:string ;
    ero:definition "An Act of Training Acquisition performed by an Agent by acquiring knowledge of the tactics, techniques, and/or strategies of the use of violence against civilians in order to attain goals that are political, religous, or ideological in nature from an Agent or communication medium that realizes a capacity derived from its affiliation with a Terrorist Organization."^^xsd:string ;
    ero:definition_source "Derived from: http://wordnetweb.princeton.edu/perl/webwn?s=terrorism"^^xsd:string ;
    rdfs:subClassOf :ActOfTrainingAcquisition .

:ActOfTerroristTrainingInstruction a owl:Class ;
    rdfs:label "Act of Terrorist Training Instruction"^^xsd:string ;
    ero:definition "An Act of Training Instruction performed by an Agent who realizes a capacity derived from the Agent's affiliation with some Terrorist Organization by imparting knowledge of the tactics, techniques, and/or strategies of the use of violence against civilians in order to attain goals that are political, religous, or ideological in nature to an Agent either directly or indirectly through some communication medium."^^xsd:string ;
    ero:definition_source "Derived from: http://wordnetweb.princeton.edu/perl/webwn?s=terrorism"^^xsd:string ;
    rdfs:subClassOf :ActOfTrainingInstruction .

:ActOfTestifying a owl:Class ;
    rdfs:label "Act of Testifying"^^xsd:string ;
    ero:definition "An Act of Representative Communication performed by making a statement about one's personal knowledge or belief."^^xsd:string ;
    ero:definition_source "Derived from: http://www.merriam-webster.com/dictionary/testify"^^xsd:string ;
    rdfs:subClassOf :ActOfRepresentativeCommunication .

:ActOfThanking a owl:Class ;
    rdfs:label "Act of Thanking"^^xsd:string ;
    ero:definition "An Act of Expressive Communication performed by expressing gratitude."^^xsd:string ;
    ero:definition_source "Derived from: http://www.merriam-webster.com/dictionary/thanks"^^xsd:string ;
    rdfs:subClassOf :ActOfExpressiveCommunication .

:ActOfToolUse a owl:Class ;
    rdfs:label "Act of Tool Use"^^xsd:string ;
    ero:definition "An Act of Artifact Employment in which some Agent uses some Tool."^^xsd:string ;
    rdfs:subClassOf :ActOfArtifactEmployment .

:ActOfVehicleUse a owl:Class ;
    rdfs:label "Act of Vehicle Use"^^xsd:string ;
    ero:definition "An Act of Artifact Employment in which some Agent uses some Vehicle."^^xsd:string ;
    rdfs:subClassOf :ActOfArtifactEmployment .

:ActOfVocationalTrainingAcquisition a owl:Class ;
    rdfs:label "Act of Vocational Training Acquisition"^^xsd:string ;
    ero:definition "An Act of Training Acquisition performed by an Agent by acquiring skills required for a specific occupation in industry, agriculture or trade from an Agent or communication medium that realizes a capacity derived from its affiliation with a Vocational School, Technical School, Trade Organization, or Industry Educational Program Provider."^^xsd:string ;
    ero:definition_source "Derived from: http://wordnetweb.princeton.edu/perl/webwn?s=vocational%20training"^^xsd:string ;
    rdfs:subClassOf :ActOfTrainingAcquisition .

:ActOfVocationalTrainingInstruction a owl:Class ;
    rdfs:label "Act of Vocational Training Instruction"^^xsd:string ;
    ero:definition "An Act of Training Instruction performed by an Agent who realizes a capacity derived from the Agent's affiliation with a Vocational School, Technical School, Trade Organization, or Industry Educational Program Provider by imparting skills required for a specific occupation in industry, agriculture or trade to an Agent either directly or indirectly through some communication medium."^^xsd:string ;
    ero:definition_source "Derived from: http://wordnetweb.princeton.edu/perl/webwn?s=vocational%20training"^^xsd:string ;
    rdfs:subClassOf :ActOfTrainingInstruction .

:ActOfVolunteering a owl:Class ;
    rdfs:label "Act of Volunteering"^^xsd:string ;
    ero:definition "An Act of Commissive Communication performed by voluntarily undertaking or expressing a willingness to undertake a service."^^xsd:string ;
    ero:definition_source "Derived from: http://www.merriam-webster.com/dictionary/volunteer"^^xsd:string ;
    ero:example_of_usage "entering into military service voluntarily"^^xsd:string,
        "rendering a service voluntarily"^^xsd:string ;
    rdfs:subClassOf :ActOfCommissiveCommunication .

:ActOfWalking a owl:Class ;
    rdfs:label "Act of Walking"^^xsd:string ;
    ero:definition "An Act of Travel that proceeds by foot."^^xsd:string ;
    rdfs:subClassOf :ActOfTravel .

:ActOfWarning a owl:Class ;
    rdfs:label "Act of Warning"^^xsd:string ;
    ero:definition "An Act of Directive Communication performed by indicating a possible or impending danger, problem, or other unpleasant situation."^^xsd:string ;
    ero:definition_source "Derived from: http://www.merriam-webster.com/dictionary/warning"^^xsd:string ;
    rdfs:subClassOf :ActOfDirectiveCommunication .

:ActOfWeaponUse a owl:Class ;
    rdfs:label "Act of Weapon Use"^^xsd:string ;
    ero:definition "An Act of Artifact Employment in which some Agent uses some Weapon."^^xsd:string ;
    rdfs:subClassOf :ActOfArtifactEmployment .

:Behavior a owl:Class ;
    rdfs:label "Behavior"^^xsd:string ;
    ero:definition "A Process in which an Independent Continuant participates as an Agent in response to external or internal stimuli and following some pattern which is dependent upon some combination of that Independent Continuant's internal state and external conditions."^^xsd:string ;
    rdfs:subClassOf :Act .

:Birth a owl:Class ;
    rdfs:label "Birth"^^xsd:string ;
    ero:definition "A Process Boundary that starts a Biographical Life."^^xsd:string ;
    rdfs:subClassOf obo:BFO_0000035 .

:Cause a owl:Class ;
    rdfs:label "Cause"^^xsd:string ;
    ero:definition "A Process that is the cause of or one of the causes of some other Process."^^xsd:string ;
    rdfs:subClassOf obo:BFO_0000015 ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty ero:is_cause_of ;
            owl:someValuesFrom obo:BFO_0000003 ] .

:CriminalAct a owl:Class ;
    rdfs:label "Criminal Act"^^xsd:string ;
    ero:definition "An Intentional Act committed in violation of rules or laws for which some governing authority (via mechanisms such as legal systems) can prescribe a conviction."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Crime"^^xsd:string ;
    rdfs:subClassOf :IntentionalAct .

:DecreaseOfDisposition a owl:Class ;
    rdfs:label "Decrease of Disposition"^^xsd:string ;
    ero:definition "A Decrease of Realizable Entity in which some Independent Continuant has a decrease of some Disposition it bears."^^xsd:string ;
    rdfs:subClassOf :DecreaseOfRealizableEntity .

:DecreaseOfFunction a owl:Class ;
    rdfs:label "Decrease of Function"^^xsd:string ;
    ero:definition "A Decrease of Realizable Entity in which some Independent Continuant has a decrease of some Function that it bears."^^xsd:string ;
    rdfs:subClassOf :DecreaseOfRealizableEntity .

:DecreaseOfGenericallyDependentContinuant a owl:Class ;
    rdfs:label "Decrease of Generically Dependent Continuant"^^xsd:string ;
    ero:definition "A Decrease of Dependent Continuant in which some Independent Continuant has a decrease in the level of a Generically Dependent Continuant that it bears."^^xsd:string ;
    rdfs:comment "Comment: For the most part Generically Dependent Continuants do not inhere in their bearers over some continuous scale. The primary exception is religion and this class allows annotation of those cases where an Agent is described as becoming less religious. Other cases would include the decrease of an organization's bearing of some objective."^^xsd:string ;
    rdfs:subClassOf :DecreaseOfDependentContinuant .

:DecreaseOfQuality a owl:Class ;
    rdfs:label "Decrease of Quality"^^xsd:string ;
    ero:definition "A Decrease of Specifically Dependent Continuant in which some Independent Continuant has a decrease of some Quality that it bears."^^xsd:string ;
    rdfs:comment "Examples: Weight Loss, Decreasing Temperature, decreasing color intensity, loss of structural integrity"^^xsd:string ;
    rdfs:subClassOf :DecreaseOfSpecificallyDependentContinuant .

:DecreaseOfRole a owl:Class ;
    rdfs:label "Decrease of Role"^^xsd:string ;
    ero:definition "A Decrease of Realizable Entity in which some Independent Continuant has a decrease of some Role that it bears."^^xsd:string ;
    rdfs:subClassOf :DecreaseOfRealizableEntity .

:Economy a owl:Class ;
    rdfs:label "Economy"^^xsd:string ;
    ero:definition "A Process Aggregate comprising the production, exchange, distribution and consumption of goods that occurs at some Geopolitical Entity."^^xsd:string ;
    rdfs:subClassOf :ProcessAggregate .

:EffectOfLocationChange a owl:Class ;
    rdfs:label "Effect of Location Change"^^xsd:string ;
    ero:definition "An Effect caused by some Act of Location Change and which results in an Object being located in a different place."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ero:caused_by ;
            owl:someValuesFrom :ActOfLocationChange ],
        [ a owl:Restriction ;
            owl:onProperty <http://www.obofoundry.org/ro/ro.owl#part_of> ;
            owl:someValuesFrom :ActOfLocationChange ],
        :Effect .

:Election a owl:Class ;
    rdfs:label "Election"^^xsd:string ;
    ero:alternative_label "Act of Political Election"^^xsd:string ;
    ero:definition "A Social Act by which a population chooses an individual to hold public office."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Election"^^xsd:string ;
    rdfs:subClassOf :SocialAct .

:EmailMessaging a owl:Class ;
    rdfs:label "Email Messaging"^^xsd:string ;
    ero:alternative_label "Act of Emailing"^^xsd:string ;
    ero:definition "An Act of Communication by Media in which text-based communication occurs between two or more people using personal computers or other devices using some Email Client Software conveyed over the Internet."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Email"^^xsd:string ;
    rdfs:subClassOf :ActOfCommunicationByMedia .

:EnhancedStasis a owl:Class ;
    rdfs:label "Enhanced Stasis"^^xsd:string ;
    ero:alternative_label "Enhanced"^^xsd:string ;
    ero:definition "A Stasis of Specifically Dependent Continuant in which some Independent Continuant bears a Quality or Realizable Entity that has undergone improvement (i.e., an increase or gain) due to a previous action or event such that the Independent Continuant is now of greater value, usefulness, or functionality."^^xsd:string ;
    ero:definition_source "Derived from: http://www.thefreedictionary.com/enhance"^^xsd:string ;
    rdfs:subClassOf :StasisOfSpecificallyDependentContinuant .

:EssentialServiceProvision a owl:Class ;
    rdfs:label "Essential Service Provision"^^xsd:string ;
    ero:definition "A Process Aggregate comprising the delivery of one or more essential services."^^xsd:string ;
    rdfs:comment "An essential service is defined here as being one of: medical care, food and water, shelter, or sanitation."^^xsd:string ;
    rdfs:subClassOf :ProcessAggregate .

:FacsimileTransmission a owl:Class ;
    rdfs:label "Facsimile Transmission"^^xsd:string ;
    ero:alternative_label "Act of Facsimile Transmission"^^xsd:string,
        "Act of Faxing"^^xsd:string ;
    ero:definition "An Act of Communication by Media in which the Information Content Entity that inheres in a Document is transmitted over a Telephone Network."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Facsimile_machine"^^xsd:string ;
    rdfs:subClassOf :ActOfCommunicationByMedia .

:FixedLineNetworkTelephoneCall a owl:Class ;
    rdfs:label "Fixed Line Network Telephone Call"^^xsd:string ;
    ero:definition "A Telephone Call transmitted over a telephone network where the telephones are wired into a single telephone exchange."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Telephone_network"^^xsd:string ;
    rdfs:subClassOf :TelephoneCall .

:Funeral a owl:Class ;
    rdfs:label "Funeral"^^xsd:string ;
    ero:alternative_label "Act of Funeral Ceremony"^^xsd:string ;
    ero:definition "An Act of Ceremony in which the Life of a Person who has died is celebrated, sanctified, or remembered."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Funeral"^^xsd:string ;
    rdfs:subClassOf :ActOfCeremony .

:GainOfDisposition a owl:Class ;
    rdfs:label "Gain of Disposition"^^xsd:string ;
    ero:definition "A Gain of Realizable Entity in which some Independent Continuant becomes the bearer of some Disposition."^^xsd:string ;
    rdfs:comment "This class should be used to demarcate the start of the temporal interval (through the occurs_on property) that the Entity bears the Disposition."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <http://www.obofoundry.org/ro/ro.owl#has_participant> ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( obo:BFO_0000004 [ a owl:Restriction ;
                                owl:onProperty ero:has_disposition ;
                                owl:someValuesFrom obo:BFO_0000016 ] ) ] ],
        :GainOfRealizableEntity .

:GainOfFunction a owl:Class ;
    rdfs:label "Gain of Function"^^xsd:string ;
    ero:definition "A Gain of Realizable Entity in which some Independent Continuant becomes the bearer of some Function."^^xsd:string ;
    rdfs:comment "This class should be used to demarcate the start of the temporal interval (through the occurs_on property) that the Entity bears the Function."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <http://www.obofoundry.org/ro/ro.owl#has_participant> ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( obo:BFO_0000004 [ a owl:Restriction ;
                                owl:onProperty ero:has_function ;
                                owl:someValuesFrom obo:BFO_0000034 ] ) ] ],
        :GainOfRealizableEntity .

:GainOfGenericallyDependentContinuant a owl:Class ;
    rdfs:label "Gain of Generically Dependent Continuant"^^xsd:string ;
    ero:definition "A Gain of Dependent Continuant wherein some Independent Continuant becomes the bearer of some Generically Dependent Continuant."^^xsd:string ;
    rdfs:comment "Examples: A Person forms a Plan, A Person is initiated into a Religion, A photgraphic image is produced."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <http://www.obofoundry.org/ro/ro.owl#has_participant> ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( obo:BFO_0000004 [ a owl:Restriction ;
                                owl:onProperty ero:bearer_of ;
                                owl:someValuesFrom obo:BFO_0000031 ] ) ] ],
        :GainOfDependentContinuant .

:GainOfQuality a owl:Class ;
    rdfs:label "Gain of Quality"^^xsd:string ;
    ero:definition "A Gain of Specifically Dependent Continuant in which an Independent Continuant becomes the bearer of some Quality."^^xsd:string ;
    rdfs:comment "Examples: A person becoming pregnant."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <http://www.obofoundry.org/ro/ro.owl#has_participant> ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( obo:BFO_0000004 [ a owl:Restriction ;
                                owl:onProperty ero:has_quality ;
                                owl:someValuesFrom obo:BFO_0000019 ] ) ] ],
        :GainOfSpecificallyDependentContinuant .

:GainOfRole a owl:Class ;
    rdfs:label "Gain of Role"^^xsd:string ;
    ero:definition "A Gain of Realizable Entity in which some Independent Continuant becomes bearer of some Role."^^xsd:string ;
    rdfs:comment "This class should be used to demarcate the start of the temporal interval (through the occurs_on property) that the Entity bears the Role."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <http://www.obofoundry.org/ro/ro.owl#has_participant> ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( obo:BFO_0000004 [ a owl:Restriction ;
                                owl:onProperty ero:has_role ;
                                owl:someValuesFrom obo:BFO_0000023 ] ) ] ],
        :GainOfRealizableEntity .

:IncreaseOfDisposition a owl:Class ;
    rdfs:label "Increase of Disposition"^^xsd:string ;
    ero:definition "An Increase of Realizable Entity in which some Independent Continuant has an increase of some Disposition that it bears."^^xsd:string ;
    rdfs:subClassOf :IncreaseOfRealizableEntity .

:IncreaseOfFunction a owl:Class ;
    rdfs:label "Increase of Function"^^xsd:string ;
    ero:definition "An Increase of Realizable Entity in which some Independent Continuant has an increase of some Function that it bears."^^xsd:string ;
    rdfs:subClassOf :IncreaseOfRealizableEntity .

:IncreaseOfGenericallyDependentContinuant a owl:Class ;
    rdfs:label "Increase of Generically Dependent Continuant"^^xsd:string ;
    ero:definition "An Increase of Dependent Continuant in which some Independent Continuant has an increase of some Generically Dependent Continuant that it bears."^^xsd:string ;
    rdfs:subClassOf :IncreaseOfDependentContinuant .

:IncreaseOfQuality a owl:Class ;
    rdfs:label "Increase of Quality"^^xsd:string ;
    ero:definition "An Increase of Specifically Dependent Continuant in which some Indpendent Continuant has an increase of some Quality that it bears."^^xsd:string ;
    rdfs:subClassOf :IncreaseOfSpecificallyDependentContinuant .

:IncreaseOfRole a owl:Class ;
    rdfs:label "Increase of Role"^^xsd:string ;
    ero:definition "An Increase of Realizable Entity in which some Independent Continuant has an increase of some Role that it bears."^^xsd:string ;
    rdfs:subClassOf :IncreaseOfRealizableEntity .

:InstantMessaging a owl:Class ;
    rdfs:label "Instant Messaging"^^xsd:string ;
    ero:alternative_label "Act of IMing"^^xsd:string,
        "Act of Instant Messaging"^^xsd:string ;
    ero:definition "An Act of Communication by Media in which real-time direct text-based communication occurs between two or more people using personal computers or other devices, along with shared Instant Messaging Client Software conveyed over a network, such as the Internet."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Instant_messaging#Clients"^^xsd:string ;
    rdfs:subClassOf :ActOfCommunicationByMedia .

:LegalSystemAct a owl:Class ;
    rdfs:label "Legal System Act"^^xsd:string ;
    ero:definition "An Intentional Act performed by an Agent that realizes their role within the context of a legal system of some jurisdiction."^^xsd:string ;
    ero:definition_source "Derived from: http://www.id.uscourts.gov/glossary.htm"^^xsd:string ;
    rdfs:subClassOf :IntentionalAct .

:LifeEvent a owl:Class ;
    rdfs:label "Life Event"^^xsd:string ;
    ero:definition "A Fiat Process Part participated in by the Agent of some Biographical Life."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <http://www.obofoundry.org/ro/ro.owl#part_of> ;
            owl:someValuesFrom :BiographicalLife ],
        :FiatProcessPart .

:LossOfDisposition a owl:Class ;
    rdfs:label "Loss of Disposition"^^xsd:string ;
    ero:definition "A Loss of Realizable Entity in which some Independent Continuant ceases to be the bearer of some Disposition."^^xsd:string ;
    rdfs:subClassOf :LossOfRealizableEntity .

:LossOfFunction a owl:Class ;
    rdfs:label "Loss of Function"^^xsd:string ;
    ero:definition "A Loss of Realizable Entity in which some Independent Continuant ceases to be the bearer of some Function."^^xsd:string ;
    rdfs:subClassOf :LossOfRealizableEntity .

:LossOfGenericallyDependentContinuant a owl:Class ;
    rdfs:label "Loss of Generically Dependent Continuant"^^xsd:string ;
    ero:definition "A Loss of Dependent Continuant in which some Independent Continuant ceases to be the bearer of some Generically Dependent Continuant."^^xsd:string ;
    rdfs:subClassOf :LossOfDependentContinuant .

:LossOfQuality a owl:Class ;
    rdfs:label "Loss of Quality"^^xsd:string ;
    ero:definition "A Loss of Specifically Dependent Continuant in which some Independent Continuant ceases to be the bearer of some Quality."^^xsd:string ;
    rdfs:subClassOf :LossOfSpecificallyDependentContinuant .

:LossOfRole a owl:Class ;
    rdfs:label "Loss of Role"^^xsd:string ;
    ero:definition "A Loss of Realizable Entity in which some Independent Continuant ceases to be the bearer of some Role."^^xsd:string ;
    rdfs:subClassOf :LossOfRealizableEntity .

:Mailing a owl:Class ;
    rdfs:label "Mailing"^^xsd:string ;
    ero:alternative_label "Act of Mailing"^^xsd:string ;
    ero:definition "An Act of Communication by Media in which information and tangible objects, usually written documents, are delivered to destinations around the world."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Mail"^^xsd:string ;
    rdfs:subClassOf :ActOfCommunicationByMedia .

:Married a owl:Class ;
    rdfs:label "Married"^^xsd:string ;
    ero:alternative_label "Married Stasis"^^xsd:string ;
    ero:definition "A Stasis of Generically Dependent Continuant that consists of a socially, culturally, or ritually recognized union or legal contract between spouses that establishes rights and obligations between them, between them and their children, and between them and their in-laws."^^xsd:string ;
    ero:definition_source """Derived from: Haviland, William A.; Prins, Harald E. L.; McBride, Bunny; Walrath, Dana (2011). Cultural Anthropology: The Human Challenge (13th ed.). Cengage Learning. ISBN 978-0-495-81178-7.
(See: http://en.wikipedia.org/wiki/Marriage)"""^^xsd:string ;
    rdfs:subClassOf :StasisOfGenericallyDependentContinuant .

:NaturalProcess a owl:Class ;
    rdfs:label "Natural Process"^^xsd:string ;
    ero:definition "A Process existing in or produced by nature; rather than by the intent of human beings."^^xsd:string ;
    ero:definition_source "Derived from: http://www.thefreedictionary.com/natural+process"^^xsd:string ;
    rdfs:subClassOf obo:BFO_0000015 .

:NominalStasis a owl:Class ;
    rdfs:label "Nominal Stasis"^^xsd:string ;
    ero:alternative_label "Nominal"^^xsd:string ;
    ero:definition "A Stasis of Specifically Dependent Continuant in which some Independent Continuant bears a Quality or Realizable Entity that has a level of intensity or functionality that falls within the designed, expected, or acceptable range such that the Independent Continuant is of normal value, usefulness, or functionality."^^xsd:string ;
    ero:definition_source "Derived from: https://en.oxforddictionaries.com/definition/nominal (Definition 6)"^^xsd:string ;
    rdfs:subClassOf :StasisOfSpecificallyDependentContinuant .

:PrivateNetworkTelephoneCall a owl:Class ;
    rdfs:label "Private Network Telephone Call"^^xsd:string ;
    ero:definition "A Telephone Call transmitted over a network where a closed group of telephones are connected primarily to each other and use a gateway to reach the outside world, usually used inside companies and call centers (a.k.a. private branch exchange (PBX))."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Telephone_network"^^xsd:string ;
    rdfs:subClassOf :TelephoneCall .

:ProcessHistory a owl:Class ;
    rdfs:label "Process History"^^xsd:string ;
    ero:definition "A Process Aggregate that consists of the sum of all Processes that are part of or otherwise involved in the primary Process (i.e., the Process for which the history is being represented) and which are bounded by the Spatiotemporal Region in which the primary Process occurs."^^xsd:string ;
    ero:elucidation "While a History is, roughly, the sum of all Processes involving a particular Independent Continuant, a Process History is, roughly, the sum of all Processes involved with a particular Process, which often includes other Processes that the Agents and Participants in the primary Process are participating in during the same Temporal Region."^^xsd:string ;
    ero:example_of_usage "The Process History of a particular passenger flight includes the processes of refueling, takeoff, landing, passenger and luggage loading and unloading, flight attendants serving refreshments and peanuts, passengers sitting, pilots operating the flight controls, air rushing past the plane, communications with air traffic control, sensor operations, and many more."^^xsd:string,
        "The Process History of a particular sporting event includes the processes of the fans arriving, cheering, booing, holding up signs, buying food and merchandise, and leaving as well as the processes that the coaches, players, and referees engage in during the sporting event."^^xsd:string ;
    rdfs:subClassOf :ProcessAggregate .

:ProcessRecurrenceAggregate a owl:Class ;
    rdfs:label "Process Recurrence Aggregate"^^xsd:string ;
    ero:definition "A Process Aggregate that consists of the collection of subprocesses that are all instances of a single type of Process and are parts of a given History or Process History."^^xsd:string ;
    rdfs:subClassOf :ProcessAggregate .

:RadioBroadcast a owl:Class ;
    rdfs:label "Radio Broadcast"^^xsd:string ;
    ero:alternative_label "Act of Radio Broadcasting"^^xsd:string ;
    ero:definition "An Act of Communciation by Media that is transmitted to an audience through a radio network."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Radio_network"^^xsd:string ;
    rdfs:subClassOf :ActOfCommunicationByMedia .

:StasisOfDisposition a owl:Class ;
    rdfs:label "Stasis of Disposition"^^xsd:string ;
    ero:definition "A Stasis of Realizable Entity in which some Independent Continuant bears some Disposition that remains unchanged during a Temporal Interval."^^xsd:string ;
    rdfs:subClassOf :StasisOfRealizableEntity .

:StasisOfFunction a owl:Class ;
    rdfs:label "Stasis of Function"^^xsd:string ;
    ero:definition "A Stasis of Realizable Entity in which some Independent Continuant bears some Function that remains unchanged during a Temporal Interval."^^xsd:string ;
    rdfs:subClassOf :StasisOfRealizableEntity .

:StasisOfQuality a owl:Class ;
    rdfs:label "Stasis of Quality"^^xsd:string ;
    ero:definition "A Stasis of Specifically Dependent Continuant in which some Independent Continuant bears some Quality that remains unchanged during a Temporal Interval."^^xsd:string ;
    rdfs:subClassOf :StasisOfSpecificallyDependentContinuant .

:StasisOfRole a owl:Class ;
    rdfs:label "Stasis of Role"^^xsd:string ;
    ero:definition "A Stasis of Realizable Entity in which some Independent Continuant bears some Role that remains unchanged during a Temporal Interval."^^xsd:string ;
    rdfs:subClassOf :StasisOfRealizableEntity .

:Target a owl:Class ;
    rdfs:label "Target"^^xsd:string ;
    ero:definition "An Entity that has the role of Target in virtue of its being involved in the Objective of some Intentional Act or in the described outcome of some Plan."^^xsd:string ;
    rdfs:subClassOf obo:BFO_0000001 .

:TelevisionBroadcast a owl:Class ;
    rdfs:label "Television Broadcast"^^xsd:string ;
    ero:alternative_label "Act of Television Broadcasting"^^xsd:string ;
    ero:definition "An Act of Communciation by Media that is transmitted to an audience through a television network."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Television_network"^^xsd:string ;
    rdfs:subClassOf :ActOfCommunicationByMedia .

:TextMessaging a owl:Class ;
    rdfs:label "Text Messaging"^^xsd:string ;
    ero:alternative_label "Act of Text Messaging"^^xsd:string,
        "Act of Texting"^^xsd:string ;
    ero:definition "An Act of Communication by Media involving the exchange of brief written messages between fixed-line phone or mobile phone and fixed or portable devices over a network."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Text_messaging"^^xsd:string ;
    rdfs:subClassOf :ActOfCommunicationByMedia .

:UnintentionalAct a owl:Class ;
    rdfs:label "Unintentional Act"^^xsd:string ;
    ero:definition "An Act in which at least one Agent plays a causative role and which is not prescribed by some Objective held by any of the Agents."^^xsd:string ;
    rdfs:subClassOf :Act .

:Webcast a owl:Class ;
    rdfs:label "Webcast"^^xsd:string ;
    ero:alternative_label "Act of Webcasting"^^xsd:string ;
    ero:definition "An Act of Communciation by Media transmitted to an audience over the Internet using streaming media technology to distribute a single content source that may be distributed live or on demand."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Webcast"^^xsd:string ;
    rdfs:comment "Essentially, webcasting is âbroadcastingâ over the Internet."^^xsd:string ;
    rdfs:subClassOf :ActOfCommunicationByMedia .

:Wedding a owl:Class ;
    rdfs:label "Wedding"^^xsd:string ;
    ero:alternative_label "Act of Wedding Ceremony"^^xsd:string ;
    ero:definition "An Act of Ceremony in which two Persons are united in Marriage or a similar institution."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Wedding"^^xsd:string ;
    rdfs:subClassOf :ActOfCeremony .

:WirelessNetworkTelephoneCall a owl:Class ;
    rdfs:label "Wireless Network Telephone Call"^^xsd:string ;
    ero:definition "A Telephone Call transmitted over a network where the telephones are mobile and can move anywhere within the coverage area."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Telephone_network"^^xsd:string ;
    rdfs:subClassOf :TelephoneCall .

:WoundedStasis a owl:Class ;
    rdfs:label "Wounded Stasis"^^xsd:string ;
    ero:alternative_label "Wounded"^^xsd:string ;
    ero:definition "A Damaged Stasis in which a Person or other organism is the bearer of a Quality or Realizable Entity that has suffered impairment (i.e., a decrease or loss) due to an injuring event."^^xsd:string ;
    rdfs:subClassOf :DamagedStasis .

:ActOfArtifactProcessing a owl:Class ;
    rdfs:label "Act of Artifact Processing"^^xsd:string ;
    ero:definition "An Intentional Act of performing a series of mechanical or chemical operations on something in order to change or preserve it."^^xsd:string ;
    rdfs:subClassOf :IntentionalAct .

:ActOfDeceptiveCommunication a owl:Class ;
    rdfs:label "Act of Deceptive Communication"^^xsd:string ;
    ero:definition "Ac Act of Communication intended to mislead the audience by distortion or falsification of evidence and induce a reaction that is prejudicial to the interests of the audience."^^xsd:string ;
    ero:definition_source "Derived from: JC3IEDM-MIRD-DMWG-Edition_3.0.2_20090514"^^xsd:string ;
    rdfs:subClassOf :ActOfCommunication .

:ActOfEmployment a owl:Class ;
    rdfs:label "Act of Employment"^^xsd:string ;
    ero:definition "An Act of Association wherein an Organization assigns a set of activities to some Person to be performed in exchange for financial compensation."^^xsd:string ;
    rdfs:subClassOf :ActOfAssociation .

:ActOfHomicide a owl:Class ;
    rdfs:label "Act of Homicide"^^xsd:string ;
    ero:definition "An Act of Violence which causes the unlawful killing of another person."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ero:is_cause_of ;
            owl:someValuesFrom :Death ],
        :ActOfViolence .

:ActOfMurder a owl:Class ;
    rdfs:label "Act of Murder"^^xsd:string ;
    ero:definition "An Act of Homicide with malice aforethought."^^xsd:string ;
    rdfs:subClassOf :ActOfHomicide .

:ActOfPossession a owl:Class ;
    rdfs:label "Act of Possession"^^xsd:string ;
    ero:definition "An Intentional Act in which an agent intentionally exercises control towards a thing."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Possession_(law)"^^xsd:string ;
    rdfs:comment "In all cases, to possess something, an agent must have an intention to possess it."^^xsd:string ;
    rdfs:subClassOf :IntentionalAct .

:ActOfPromising a owl:Class ;
    rdfs:label "Act of Promising"^^xsd:string ;
    ero:definition "An Act of Commissive Communication performed by declaring that the promising agent will do, or refrain from doing, the specified action."^^xsd:string ;
    ero:definition_source "Derived from: http://www.merriam-webster.com/dictionary/promise"^^xsd:string ;
    rdfs:subClassOf :ActOfCommissiveCommunication .

:BiographicalLife a owl:Class ;
    rdfs:label "Biographical Life"^^xsd:string ;
    ero:definition "An Act composed of the Life Events that occur during the course of existence of an Agent (i.e. Person or Organization)."^^xsd:string ;
    rdfs:subClassOf :Act .

:DamagedStasis a owl:Class ;
    rdfs:label "Damaged Stasis"^^xsd:string ;
    ero:alternative_label "Damaged"^^xsd:string ;
    ero:definition "A Stasis of Specifically Dependent Continuant in which some Independent Continuant bears a Quality or Realizable Entity that has suffered impairment (i.e., a decrease or loss) due to a previous action or event such that the Independent Continuant is now of lesser value, usefulness, or functionality."^^xsd:string ;
    ero:definition_source "Derived from: http://www.thefreedictionary.com/damaged"^^xsd:string ;
    rdfs:subClassOf :StasisOfSpecificallyDependentContinuant .

:Effect a owl:Class ;
    rdfs:label "Effect"^^xsd:string ;
    ero:alternative_label "Consequence"^^xsd:string ;
    ero:definition "A Process that follows and is caused by some previous Process."^^xsd:string ;
    ero:definition_source "Derived from: http://wordnetweb.princeton.edu/perl/webwn?s=effect"^^xsd:string ;
    rdfs:subClassOf obo:BFO_0000015 ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty ero:caused_by ;
            owl:someValuesFrom obo:BFO_0000003 ] .

:FiatProcessPart a owl:Class ;
    rdfs:label "Fiat Process Part"^^xsd:string ;
    ero:definition "A Process that is part of another Process and is differentiated from the whole conceptually."^^xsd:string ;
    rdfs:subClassOf obo:BFO_0000015 .

:StasisOfGenericallyDependentContinuant a owl:Class ;
    rdfs:label "Stasis of Generically Dependent Continuant"^^xsd:string ;
    ero:definition "A Stasis in which some Independent Continuant bears some Generically Dependent Continuant that remains unchanged during a Temporal Interval."^^xsd:string ;
    rdfs:subClassOf :Stasis .

<http://www.obofoundry.org/ro/ro.owl#part_of> a owl:ObjectProperty .

:ActOfDeclarativeCommunication a owl:Class ;
    rdfs:label "Act of Declarative Communication"^^xsd:string ;
    ero:definition "An Act of Communication that changes the reality in accord with the proposition of the declaration."^^xsd:string ;
    ero:definition_source "Derived (loosely) from: John Searle's Speech Acts: An Essay in the Philosophy of Language"^^xsd:string ;
    rdfs:comment "Examples: Baptism, Sentencing a person to imprisonment, Pronouncing a couple husband and wife, Declaring war"^^xsd:string ;
    rdfs:subClassOf :ActOfCommunication .

:ActOfInhabitancy a owl:Class ;
    rdfs:label "Act of Inhabitancy"^^xsd:string ;
    ero:definition "An Intentional Act in which a Person lives at a Site for a period of time that may be as short as 1 day/night or as long as the Person's entire life."^^xsd:string ;
    ero:definition_source "Derived from: http://www.merriam-webster.com/dictionary/inhabit"^^xsd:string ;
    rdfs:subClassOf :IntentionalAct .

:ActOfIntelligenceGathering a owl:Class ;
    rdfs:label "Act of Intelligence Gathering"^^xsd:string ;
    ero:definition "An Intentional Act of producing and/or gathering information by a government or other Organization to guide decisions and actions by answering questions or obtaining advance warning of events and movements deemed to be important or otherwise relevant."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Intelligence_(information_gathering)"^^xsd:string ;
    rdfs:subClassOf :IntentionalAct .

:ActOfMotion a owl:Class ;
    rdfs:label "Act of Motion"^^xsd:string ;
    ero:definition "An Intentional Act by which an Agent causes the position or location of some Object to change."^^xsd:string ;
    ero:definition_source "Derived from: http://www.merriam-webster.com/dictionary/motion"^^xsd:string ;
    rdfs:subClassOf :IntentionalAct .

:ActOfTraining a owl:Class ;
    rdfs:label "Act of Training"^^xsd:string ;
    ero:definition "An Intentional Act in which knowledge, skills or values are imparted from one or more Agents to at least one other Agent."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Education"^^xsd:string ;
    rdfs:subClassOf :IntentionalAct .

:ActOfTravel a owl:Class ;
    rdfs:label "Act of Travel"^^xsd:string ;
    ero:definition "An Act of Location Change wherein one or more Persons move between relatively distant geographical locations for any purpose and any duration, with or without any means of transport."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Travel"^^xsd:string ;
    rdfs:subClassOf :ActOfLocationChange .

:ActOfViolence a owl:Class ;
    rdfs:label "Act of Violence"^^xsd:string ;
    ero:definition "An Intentional Act of causing harm to oneself or to another through the use of physical or verbal force."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Violence"^^xsd:string ;
    rdfs:subClassOf :IntentionalAct .

:Death a owl:Class ;
    rdfs:label "Death"^^xsd:string ;
    ero:definition "A Process Boundary that ends a Biographical Life."^^xsd:string ;
    rdfs:subClassOf obo:BFO_0000035 .

:DecreaseOfDependentContinuant a owl:Class ;
    rdfs:label "Decrease of Dependent Continuant"^^xsd:string ;
    ero:definition "A Change in which some Independent Continuant has a decrease in the level of some Dependent Continuant that it bears."^^xsd:string ;
    rdfs:subClassOf :Change .

:DecreaseOfSpecificallyDependentContinuant a owl:Class ;
    rdfs:label "Decrease of Specifically Dependent Continuant"^^xsd:string ;
    ero:definition "A Decrease of Dependent Continuant in which some Independent Continuant has a decrease in some Specifically Dependent Continuant that it bears."^^xsd:string ;
    rdfs:subClassOf :DecreaseOfDependentContinuant .

:GainOfDependentContinuant a owl:Class ;
    rdfs:label "Gain of Dependent Continuant"^^xsd:string ;
    ero:definition "A Change in which an Independent Continuant becomes the bearer of some Dependent Continuant."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <http://www.obofoundry.org/ro/ro.owl#has_participant> ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( obo:BFO_0000004 [ a owl:Restriction ;
                                owl:onProperty ero:bearer_of ;
                                owl:someValuesFrom [ a owl:Class ;
                                        owl:unionOf ( obo:BFO_0000020 obo:BFO_0000031 ) ] ] ) ] ],
        :Change .

:GainOfSpecificallyDependentContinuant a owl:Class ;
    rdfs:label "Gain of Specifically Dependent Continuant"^^xsd:string ;
    ero:definition "A Gain of Dependent Continuant in which some Independent Continuant becomes the bearer of some Specifically Dependent Continuant."^^xsd:string ;
    rdfs:comment "Examples: A Person becomes pregnant (gain of quality), A person becomes forgetful (gain of disposition), A vehicle becomes amphibious (gain of function), A Person becomes a Database Administrator (gain of role)."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <http://www.obofoundry.org/ro/ro.owl#has_participant> ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( obo:BFO_0000004 [ a owl:Restriction ;
                                owl:onProperty ero:bearer_of ;
                                owl:someValuesFrom obo:BFO_0000020 ] ) ] ],
        :GainOfDependentContinuant .

:IncreaseOfDependentContinuant a owl:Class ;
    rdfs:label "Increase of Dependent Continuant"^^xsd:string ;
    ero:definition "A Change in which some Independent Continuant has an increase in the level of some Dependent Continuant that it bears."^^xsd:string ;
    rdfs:subClassOf :Change .

:IncreaseOfSpecificallyDependentContinuant a owl:Class ;
    rdfs:label "Increase of Specifically Dependent Continuant"^^xsd:string ;
    ero:definition "An Increase of Dependent Continuant in which some Independent Continuant has an increase of some Specifically Dependent Continuant that it bears."^^xsd:string ;
    rdfs:subClassOf :IncreaseOfDependentContinuant .

:LossOfDependentContinuant a owl:Class ;
    rdfs:label "Loss of Dependent Continuant"^^xsd:string ;
    ero:definition "A Change in which some Independent Continuant ceases to be the bearer of some Dependent Continuant."^^xsd:string ;
    rdfs:subClassOf :Change .

:LossOfSpecificallyDependentContinuant a owl:Class ;
    rdfs:label "Loss of Specifically Dependent Continuant"^^xsd:string ;
    ero:definition "A Loss of Dependent Continuant in which some Independent Continuant ceases to be the bearer of some Specifically Dependent Continuant."^^xsd:string ;
    rdfs:subClassOf :LossOfDependentContinuant .

:Stasis a owl:Class ;
    rdfs:label "Stasis"^^xsd:string ;
    ero:definition "A Process in which some Independent Continuant endures and one or more of the dependent entities it bears does not change in kind or intensity."^^xsd:string ;
    rdfs:subClassOf obo:BFO_0000015 .

:ActOfCeremony a owl:Class ;
    rdfs:label "Act of Ceremony"^^xsd:string ;
    ero:definition "A Social Act of ritual significance, performed on a special occasion."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Ceremony"^^xsd:string ;
    rdfs:subClassOf :SocialAct .

:ActOfPurchasing a owl:Class ;
    rdfs:label "Act of Purchasing"^^xsd:string ;
    ero:definition "An Act of Financial Instrument Use wherein a Financial Instrument is used by an Agent (the purchaser) to acquire a good or service from another agent (the provider)."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Purchase"^^xsd:string ;
    rdfs:subClassOf :ActOfFinancialInstrumentUse .

:DecreaseOfRealizableEntity a owl:Class ;
    rdfs:label "Decrease of Realizable Entity"^^xsd:string ;
    ero:definition "A Decrease of Specifically Dependent Continuant in which some Independent Continuant has a decrease of some Realizable Entity that it bears."^^xsd:string ;
    rdfs:subClassOf :DecreaseOfSpecificallyDependentContinuant .

:GainOfRealizableEntity a owl:Class ;
    rdfs:label "Gain of Realizable Entity"^^xsd:string ;
    ero:definition "A Gain of Specifically Dependent Continuant in which some Independent Continuant becomes the bearer of some Realizable Entity."^^xsd:string ;
    rdfs:comment "Examples: An informant becomes unreliable (disposition), A person begins to speak French (function), a person becomes a welder (role)."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <http://www.obofoundry.org/ro/ro.owl#has_participant> ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( obo:BFO_0000004 [ a owl:Restriction ;
                                owl:onProperty ero:bearer_of ;
                                owl:someValuesFrom obo:BFO_0000017 ] ) ] ],
        :GainOfSpecificallyDependentContinuant .

:IncreaseOfRealizableEntity a owl:Class ;
    rdfs:label "Increase of Realizable Entity"^^xsd:string ;
    ero:definition "An Increase of Specifically Dependent Continuant in which some Independent Continuant has an increase of some Realizable Entity that it bears."^^xsd:string ;
    rdfs:subClassOf :IncreaseOfSpecificallyDependentContinuant .

:LossOfRealizableEntity a owl:Class ;
    rdfs:label "Loss of Realizable Entity"^^xsd:string ;
    ero:definition "A Loss of Specifically Dependent Continuant in which some Independent Continuant ceases to be the bearer of some Realizable Entity."^^xsd:string ;
    rdfs:subClassOf :LossOfSpecificallyDependentContinuant .

:StasisOfRealizableEntity a owl:Class ;
    rdfs:label "Stasis of Realizable Entity"^^xsd:string ;
    ero:definition "A Stasis of Specifically Dependent Continuant in which some Independent Continuant bears some Realizable Entity that remains unchanged during a Temporal Interval."^^xsd:string ;
    rdfs:subClassOf :StasisOfSpecificallyDependentContinuant .

:TelephoneCall a owl:Class ;
    rdfs:label "Telephone Call"^^xsd:string ;
    ero:alternative_label "Act of Telephone Calling"^^xsd:string ;
    ero:definition "An Act of Communciation by Media transmitted over a telephone network to two or more Persons."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Telephone_network"^^xsd:string ;
    rdfs:subClassOf :ActOfCommunicationByMedia .

:Act a owl:Class ;
    rdfs:label "Act"^^xsd:string ;
    ero:definition "A Process in which at least one Agent plays a causative role."^^xsd:string ;
    rdfs:subClassOf obo:BFO_0000015 .

:ActOfCommissiveCommunication a owl:Class ;
    rdfs:label "Act of Commissive Communication"^^xsd:string ;
    ero:definition "An Act of Communication that commits a speaker to some future action."^^xsd:string ;
    ero:definition_source "Derived (loosely) from: John Searle's Speech Acts: An Essay in the Philosophy of Language"^^xsd:string ;
    rdfs:comment "Examples: agreeing, betting, guaranteeing, inviting, offering, promising, swearing, volunteering"^^xsd:string ;
    rdfs:subClassOf :ActOfCommunication .

:ActOfDirectiveCommunication a owl:Class ;
    rdfs:label "Act of Directive Communication"^^xsd:string ;
    ero:definition "An Act of Communication that is intended to cause the hearer to take a particular action."^^xsd:string ;
    ero:definition_source "Derived (loosely) from: John Searle's Speech Acts: An Essay in the Philosophy of Language"^^xsd:string ;
    rdfs:comment "Examples: advising, admonishing, asking, begging, dismissing, excusing, forbidding, instructing, ordering, permitting, requesting, requiring, suggesting, urging, warning"^^xsd:string ;
    rdfs:subClassOf :ActOfCommunication .

:ActOfExpressiveCommunication a owl:Class ;
    rdfs:label "Act of Expressive Communication"^^xsd:string ;
    ero:definition "An Act of Communication that expresses the Speaker's attitudes and emotions towards some proposition."^^xsd:string ;
    ero:definition_source "Derived (loosely) from: John Searle's Speech Acts: An Essay in the Philosophy of Language"^^xsd:string ;
    rdfs:comment "Examples: apologizing, condoling, congratulating, greeting, thanking, accepting (acknowledging an acknowledgment)"^^xsd:string ;
    rdfs:subClassOf :ActOfCommunication .

:Change a owl:Class ;
    rdfs:label "Change"^^xsd:string ;
    ero:definition "A Process in which some independent continuant endures and 1) one or more of the dependent entities it bears increase or decrease in intensity, 2) the entity begins to bear some dependent entity or 3) the entity ceases to bear some dependent entity."^^xsd:string ;
    rdfs:subClassOf obo:BFO_0000015 .

:ProcessAggregate a owl:Class ;
    rdfs:label "Process Aggregate"^^xsd:string ;
    ero:definition "An Occurrent that is a collection of processes."^^xsd:string ;
    rdfs:subClassOf obo:BFO_0000003 .

:SocialAct a owl:Class ;
    rdfs:label "Social Act"^^xsd:string ;
    ero:definition "An Intentional Act having an objective that affects, is performed by, or is performed on behalf of, a community or group of Persons."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wiktionary.org/wiki/commually"^^xsd:string ;
    rdfs:subClassOf :IntentionalAct .

:ActOfRepresentativeCommunication a owl:Class ;
    rdfs:label "Act of Representative Communication"^^xsd:string ;
    ero:definition "An Act of Communication that commits a speaker to the truth of the expressed proposition."^^xsd:string ;
    ero:definition_source "Derived (loosely) from: John Searle's Speech Acts: An Essay in the Philosophy of Language"^^xsd:string ;
    rdfs:comment "Examples: affirming, alleging, announcing, answering, attributing, claiming, classifying, concurring, confirming, conjecturing, denying, disagreeing, disclosing, disputing, identifying, informing, insisting, predicting, ranking, reporting, stating, stipulating"^^xsd:string ;
    rdfs:subClassOf :ActOfCommunication .

:ActOfTrainingAcquisition a owl:Class ;
    rdfs:label "Act of Training Acquisition"^^xsd:string ;
    ero:definition "An Act of Training performed by an Agent by acquiring knowledge from another Agent."^^xsd:string ;
    rdfs:subClassOf :ActOfTraining .

:ActOfTrainingInstruction a owl:Class ;
    rdfs:label "Act of Training Instruction"^^xsd:string ;
    ero:definition "An Act of Training performed by an Agent by imparting knowledge to another Agent."^^xsd:string ;
    rdfs:subClassOf :ActOfTraining .

:StasisOfSpecificallyDependentContinuant a owl:Class ;
    rdfs:label "Stasis of Specifically Dependent Continuant"^^xsd:string ;
    ero:definition "A Stasis in which some Independent Continuant bears some Specifically Dependent Continuant that remains unchanged during a Temporal Interval."^^xsd:string ;
    rdfs:subClassOf :Stasis .

:ActOfAssociation a owl:Class ;
    rdfs:label "Act of Association"^^xsd:string ;
    ero:definition "A Social Act wherein an Agent unites with some other Agent in an Intentional Act, enterprise or business."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wiktionary.org/wiki/associate"^^xsd:string ;
    rdfs:subClassOf :SocialAct .

:ActOfFinancialInstrumentUse a owl:Class ;
    rdfs:label "Act of Financial Instrument Use"^^xsd:string ;
    ero:definition "An Act of Artifact Employment in which some Agent uses some Financial Instrument."^^xsd:string ;
    rdfs:subClassOf :ActOfArtifactEmployment .

:ActOfLocationChange a owl:Class ;
    rdfs:label "Act of Location Change"^^xsd:string ;
    ero:definition "An Act of Motion in which the location of some Object is changed by some Agent."^^xsd:string ;
    rdfs:subClassOf :ActOfMotion .

:ActOfMassMediaCommunication a owl:Class ;
    rdfs:label "Act of Mass Media Communication"^^xsd:string ;
    ero:definition "An Act of Communication intended to reach a large audience using a medium such as the internet, television, radio, newspaper, and magazine."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/The_media"^^xsd:string ;
    rdfs:subClassOf :ActOfCommunication .

:ActOfArtifactEmployment a owl:Class ;
    rdfs:label "Act of Artifact Employment"^^xsd:string ;
    ero:definition "An Intentional Act of using an Artifact."^^xsd:string ;
    rdfs:subClassOf :IntentionalAct .

:ActOfCommunication a owl:Class ;
    rdfs:label "Act of Communication"^^xsd:string ;
    ero:definition "An Intentional Act in which some Information Content Entity is transferred from some Agent to Another."^^xsd:string ;
    ero:definition_source "Derived from: http://en.wikipedia.org/wiki/Communication"^^xsd:string ;
    rdfs:subClassOf :IntentionalAct .

:ActOfCommunicationByMedia a owl:Class ;
    rdfs:label "Act of Communication by Media"^^xsd:string ;
    ero:definition "An Act of Communication in which some Artifact is used to transfer an Information Bearing Entity from sender(s) to receiver(s)."^^xsd:string ;
    rdfs:subClassOf :ActOfCommunication .

:IntentionalAct a owl:Class ;
    rdfs:label "Intentional Act"^^xsd:string ;
    ero:definition "An Act in which at least one Agent plays a causative role and which is prescribed by some Directive Information Content Entity held by at least one of the Agents."^^xsd:string ;
    rdfs:subClassOf :Act .

